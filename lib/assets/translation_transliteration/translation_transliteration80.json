{
    "id": "80",
    "name": "عبس",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "'abasa watawall<u>aa</u>"
        },
        {
            "id": "2",
            "text": "an j<u>aa</u>-ahu <strong>a</strong>l-a'm<u>aa</u>"
        },
        {
            "id": "3",
            "text": "wam<u>aa</u> yudriika la'allahu yazzakk<u>aa</u>"
        },
        {
            "id": "4",
            "text": "aw ya<u>dzdz</u>akkaru fatanfa'ahu <strong>al</strong><u>dzdz</u>ikr<u>aa</u>"
        },
        {
            "id": "5",
            "text": "amm<u>aa</u> mani istaghn<u>aa</u>"
        },
        {
            "id": "6",
            "text": "fa-anta lahu ta<u>sh</u>add<u>aa</u>"
        },
        {
            "id": "7",
            "text": "wam<u>aa</u> 'alayka <strong>al</strong>l<u>aa</u> yazzakk<u>aa</u>"
        },
        {
            "id": "8",
            "text": "wa-amm<u>aa</u> man j<u>aa</u>-aka yas'<u>aa</u>"
        },
        {
            "id": "9",
            "text": "wahuwa yakhsy<u>aa</u>"
        },
        {
            "id": "10",
            "text": "fa-anta 'anhu talahh<u>aa</u>"
        },
        {
            "id": "11",
            "text": "kall<u>aa</u> innah<u>aa</u> ta<u>dz</u>kira<strong>tun</strong>"
        },
        {
            "id": "12",
            "text": "faman sy<u>aa</u>-a <u>dz</u>akarah<strong>u</strong>"
        },
        {
            "id": "13",
            "text": "fii <u>sh</u>u<u>h</u>ufin mukarrama<strong>tin</strong>"
        },
        {
            "id": "14",
            "text": "marfuu'atin mu<u>th</u>ahhara<strong>tin</strong>"
        },
        {
            "id": "15",
            "text": "bi-aydii safara<strong>tin</strong>"
        },
        {
            "id": "16",
            "text": "kir<u>aa</u>min barara<strong>tin</strong>"
        },
        {
            "id": "17",
            "text": "qutila <strong>a</strong>l-ins<u>aa</u>nu m<u>aa</u> akfarah<strong>u</strong>"
        },
        {
            "id": "18",
            "text": "min ayyi syay-in khalaqah<strong>u</strong>"
        },
        {
            "id": "19",
            "text": "min nu<u>th</u>fatin khalaqahu faqaddarah<strong>u</strong>"
        },
        {
            "id": "20",
            "text": "tsumma <strong>al</strong>ssabiila yassarah<strong>u</strong>"
        },
        {
            "id": "21",
            "text": "tsumma am<u>aa</u>tahu fa-aqbarah<strong>u</strong>"
        },
        {
            "id": "22",
            "text": "tsumma i<u>dzaa</u> sy<u>aa</u>-a ansyarah<strong>u</strong>"
        },
        {
            "id": "23",
            "text": "kall<u>aa</u> lamm<u>aa</u> yaq<u>dh</u>i m<u>aa</u> amarah<strong>u</strong>"
        },
        {
            "id": "24",
            "text": "falyan<u>zh</u>uri <strong>a</strong>l-ins<u>aa</u>nu il<u>aa</u> <u>th</u>a'<u>aa</u>mih<strong>i</strong>"
        },
        {
            "id": "25",
            "text": "ann<u>aa</u> <u>sh</u>ababn<u>aa</u> <strong>a</strong>lm<u>aa</u>-a <u>sh</u>abb<u>aa</u><strong>n</strong>"
        },
        {
            "id": "26",
            "text": "tsumma syaqaqn<u>aa</u> <strong>a</strong>l-ar<u>dh</u>a syaqq<u>aa</u><strong>n</strong>"
        },
        {
            "id": "27",
            "text": "fa-anbatn<u>aa</u> fiih<u>aa</u> <u>h</u>abb<u>aa</u><strong>n</strong>"
        },
        {
            "id": "28",
            "text": "wa'inaban waqa<u>dh</u>b<u>aa</u><strong>n</strong>"
        },
        {
            "id": "29",
            "text": "wazaytuunan wanakhl<u>aa</u><strong>n</strong>"
        },
        {
            "id": "30",
            "text": "wa<u>h</u>ad<u>aa</u>-iqa ghulb<u>aa</u><strong>n</strong>"
        },
        {
            "id": "31",
            "text": "waf<u>aa</u>kihatan wa-abb<u>aa</u><strong>n</strong>"
        },
        {
            "id": "32",
            "text": "mat<u>aa</u>'an lakum wali-an'<u>aa</u>mikum"
        },
        {
            "id": "33",
            "text": "fa-i<u>dzaa</u> j<u>aa</u>-ati <strong>al</strong><u>shshaa</u>khkha<strong>tu</strong>"
        },
        {
            "id": "34",
            "text": "yawma yafirru <strong>a</strong>lmaru min akhiih<strong>i</strong>"
        },
        {
            "id": "35",
            "text": "waummihi wa-abiih<strong>i</strong>"
        },
        {
            "id": "36",
            "text": "wa<u>shaah</u>ibatihi wabaniih<strong>i</strong>"
        },
        {
            "id": "37",
            "text": "likulli imri-in minhum yawma-i<u>dz</u>in sya/nun yughniih<strong>i</strong>"
        },
        {
            "id": "38",
            "text": "wujuuhun yawma-i<u>dz</u>in musfira<strong>tun</strong>"
        },
        {
            "id": "39",
            "text": "<u>dah</u>ikatun mustabsyira<strong>tun</strong>"
        },
        {
            "id": "40",
            "text": "wawujuuhun yawma-i<u>dz</u>in 'alayh<u>aa</u> ghabara<strong>tun</strong>"
        },
        {
            "id": "41",
            "text": "tarhaquh<u>aa</u> qatara<strong>tun</strong>"
        },
        {
            "id": "42",
            "text": "ul<u>aa</u>-ika humu <strong>a</strong>lkafaratu <strong>a</strong>lfajara<strong>tu</strong>"
        }
    ]
}