{
    "id": "87",
    "name": "الأعلى",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "sabbi<u>h</u>i isma rabbika <strong>a</strong>l-a'l<u>aa</u>"
        },
        {
            "id": "2",
            "text": "<strong>al</strong>la<u>dz</u>ii khalaqa fasaww<u>aa</u>"
        },
        {
            "id": "3",
            "text": "wa<strong>a</strong>lla<u>dz</u>ii qaddara fahad<u>aa</u>"
        },
        {
            "id": "4",
            "text": "wa<strong>a</strong>lla<u>dz</u>ii akhraja <strong>a</strong>lmar'<u>aa</u>"
        },
        {
            "id": "5",
            "text": "faja'alahu ghuts<u>aa</u>-an a<u>h</u>w<u>aa</u>"
        },
        {
            "id": "6",
            "text": "sanuqri-uka fal<u>aa</u> tans<u>aa</u>"
        },
        {
            "id": "7",
            "text": "ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a <strong>al</strong>l<u>aa</u>hu innahu ya'lamu <strong>a</strong>ljahra wam<u>aa</u> yakhf<u>aa</u>"
        },
        {
            "id": "8",
            "text": "wanuyassiruka lilyusr<u>aa</u>"
        },
        {
            "id": "9",
            "text": "fa<u>dz</u>akkir in nafa'ati <strong>al</strong><u>dzdz</u>ikr<u>aa</u>"
        },
        {
            "id": "10",
            "text": "saya<u>dzdz</u>akkaru man yakhsy<u>aa</u>"
        },
        {
            "id": "11",
            "text": "wayatajannabuh<u>aa</u> <strong>a</strong>l-asyq<u>aa</u>"
        },
        {
            "id": "12",
            "text": "<strong>al</strong>la<u>dz</u>ii ya<u>sh</u>l<u>aa</u> <strong>al</strong>nn<u>aa</u>ra <strong>a</strong>lkubr<u>aa</u>"
        },
        {
            "id": "13",
            "text": "tsumma l<u>aa</u> yamuutu fiih<u>aa</u> wal<u>aa</u> ya<u>h</u>y<u>aa</u>"
        },
        {
            "id": "14",
            "text": "qad afla<u>h</u>a man tazakk<u>aa</u>"
        },
        {
            "id": "15",
            "text": "wa<u>dz</u>akara isma rabbihi fa<u>sh</u>all<u>aa</u>"
        },
        {
            "id": "16",
            "text": "bal tu/tsiruuna <strong>a</strong>l<u>h</u>ay<u>aa</u>ta <strong>al</strong>dduny<u>aa</u>"
        },
        {
            "id": "17",
            "text": "wa<strong>a</strong>l-<u>aa</u>khiratu khayrun wa-abq<u>aa</u>"
        },
        {
            "id": "18",
            "text": "inna h<u>aadzaa</u> lafii <strong>al</strong><u>shsh</u>u<u>h</u>ufi <strong>a</strong>l-uul<u>aa</u>"
        },
        {
            "id": "19",
            "text": "<u>sh</u>u<u>h</u>ufi ibr<u>aa</u>hiima wamuus<u>aa</u>"
        }
    ]
}