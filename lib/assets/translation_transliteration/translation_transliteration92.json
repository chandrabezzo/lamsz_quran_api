    "id": "92",
    "name": "الليل",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "wa<strong>a</strong>llayli i<u>dzaa</u> yaghsy<u>aa</u>"
        },
        {
            "id": "2",
            "text": "wa<strong>al</strong>nnah<u>aa</u>ri i<u>dzaa</u> tajall<u>aa</u>"
        },
        {
            "id": "3",
            "text": "wam<u>aa</u> khalaqa <strong>al</strong><u>dzdz</u>akara wa<strong>a</strong>l-unts<u>aa</u>"
        },
        {
            "id": "4",
            "text": "inna sa'yakum lasyatt<u>aa</u>"
        },
        {
            "id": "5",
            "text": "fa-amm<u>aa</u> man a'<u>th</u><u>aa</u> wa<strong>i</strong>ttaq<u>aa</u>"
        },
        {
            "id": "6",
            "text": "wa<u>sh</u>addaqa bi<strong>a</strong>l<u>h</u>usn<u>aa</u>"
        },
        {
            "id": "7",
            "text": "fasanuyassiruhu lilyusr<u>aa</u>"
        },
        {
            "id": "8",
            "text": "wa-amm<u>aa</u> man bakhila wa<strong>i</strong>staghn<u>aa</u>"
        },
        {
            "id": "9",
            "text": "waka<u>dzdz</u>aba bi<strong>a</strong>l<u>h</u>usn<u>aa</u>"
        },
        {
            "id": "10",
            "text": "fasanuyassiruhu lil'usr<u>aa</u>"
        },
        {
            "id": "11",
            "text": "wam<u>aa</u> yughnii 'anhu m<u>aa</u>luhu i<u>dzaa</u> taradd<u>aa</u>"
        },
        {
            "id": "12",
            "text": "inna 'alayn<u>aa</u> lalhud<u>aa</u>"
        },
        {
            "id": "13",
            "text": "wa-inna lan<u>aa</u> lal-<u>aa</u>khirata wa<strong>a</strong>l-uul<u>aa</u>"
        },
        {
            "id": "14",
            "text": "fa-an<u>dz</u>artukum n<u>aa</u>ran tala<u>zhzhaa</u>"
        },
        {
            "id": "15",
            "text": "l<u>aa</u> ya<u>sh</u>l<u>aa</u>h<u>aa</u> ill<u>aa</u> <strong>a</strong>l-asyq<u>aa</u>"
        },
        {
            "id": "16",
            "text": "<strong>al</strong>la<u>dz</u>ii ka<u>dzdz</u>aba watawall<u>aa</u>"
        },
        {
            "id": "17",
            "text": "wasayujannabuh<u>aa</u> <strong>a</strong>l-atq<u>aa</u>"
        },
        {
            "id": "18",
            "text": "<strong>al</strong>la<u>dz</u>ii yu/tii m<u>aa</u>lahu yatazakk<u>aa</u>"
        },
        {
            "id": "19",
            "text": "wam<u>aa</u> li-a<u>h</u>adin 'indahu min ni'matin tujz<u>aa</u>"
        },
        {
            "id": "20",
            "text": "ill<u>aa</u> ibtigh<u>aa</u>-a wajhi rabbihi <strong>a</strong>l-a'l<u>aa</u>"
        },
        {
            "id": "21",
            "text": "walasawfa yar<u>daa</u>"
        }
    ]
}