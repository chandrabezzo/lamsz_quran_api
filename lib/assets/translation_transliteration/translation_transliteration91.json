    "id": "91",
    "name": "الشمس",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "wa<strong>al</strong>sysyamsi wa<u>dh</u>u<u>haa</u>h<u>aa</u>"
        },
        {
            "id": "2",
            "text": "wa<strong>a</strong>lqamari i<u>dzaa</u> tal<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "3",
            "text": "wa<strong>al</strong>nnah<u>aa</u>ri i<u>dzaa</u> jall<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "4",
            "text": "wa<strong>a</strong>llayli i<u>dzaa</u> yaghsy<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "5",
            "text": "wa<strong>al</strong>ssam<u>aa</u>-i wam<u>aa</u> ban<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "6",
            "text": "wa<strong>a</strong>l-ar<u>dh</u>i wam<u>aa</u> <u>th</u>a<u>haa</u>h<u>aa</u>"
        },
        {
            "id": "7",
            "text": "wanafsin wam<u>aa</u> saww<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "8",
            "text": "fa-alhamah<u>aa</u> fujuurah<u>aa</u> wataqw<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "9",
            "text": "qad afla<u>h</u>a man zakk<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "10",
            "text": "waqad kh<u>aa</u>ba man dass<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "11",
            "text": "ka<u>dzdz</u>abat tsamuudu bi<u>th</u>aghw<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "12",
            "text": "i<u>dz</u>i inba'atsa asyq<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "13",
            "text": "faq<u>aa</u>la lahum rasuulu <strong>al</strong>l<u>aa</u>hi n<u>aa</u>qata <strong>al</strong>l<u>aa</u>hi wasuqy<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "14",
            "text": "faka<u>dzdz</u>abuuhu fa'aqaruuh<u>aa</u> fadamdama 'alayhim rabbuhum bi<u>dz</u>anbihim fasaww<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "15",
            "text": "wal<u>aa</u> yakh<u>aa</u>fu 'uqb<u>aa</u>h<u>aa</u>"
        }
    ]
}