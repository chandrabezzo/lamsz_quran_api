    "id": "99",
    "name": "الزلزلة",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "i<u>dzaa</u> zulzilati <strong>a</strong>l-ar<u>dh</u>u zilz<u>aa</u>lah<u>aa</u>"
        },
        {
            "id": "2",
            "text": "wa-akhrajati <strong>a</strong>l-ar<u>dh</u>u atsq<u>aa</u>lah<u>aa</u>"
        },
        {
            "id": "3",
            "text": "waq<u>aa</u>la <strong>a</strong>l-ins<u>aa</u>nu m<u>aa</u> lah<u>aa</u>"
        },
        {
            "id": "4",
            "text": "yawma-i<u>dz</u>in tu<u>h</u>additsu akhb<u>aa</u>rah<u>aa</u>"
        },
        {
            "id": "5",
            "text": "bi-anna rabbaka aw<u>haa</u> lah<u>aa</u>"
        },
        {
            "id": "6",
            "text": "yawma-i<u>dz</u>in ya<u>sh</u>duru <strong>al</strong>nn<u>aa</u>su asyt<u>aa</u>tan liyuraw a'm<u>aa</u>lahum"
        },
        {
            "id": "7",
            "text": "faman ya'mal mitsq<u>aa</u>la <u>dz</u>arratin khayran yarah<strong>u</strong>"
        },
        {
            "id": "8",
            "text": "waman ya'mal mitsq<u>aa</u>la <u>dz</u>arratin syarran yarah<strong>u</strong>"
        }
    ]
}