{
    "id": "79",
    "name": "النازعات",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "wa<strong>al</strong>nn<u>aa</u>zi'<u>aa</u>ti gharq<u>aa</u><strong>n</strong>"
        },
        {
            "id": "2",
            "text": "wa<strong>al</strong>nn<u>aa</u>syi<u>th</u><u>aa</u>ti nasy<u>th</u><u>aa</u><strong>n</strong>"
        },
        {
            "id": "3",
            "text": "wa<strong>al</strong>ss<u>aa</u>bi<u>haa</u>ti sab<u>haa</u><strong>n</strong>"
        },
        {
            "id": "4",
            "text": "fa<strong>al</strong>ss<u>aa</u>biq<u>aa</u>ti sabq<u>aa</u><strong>n</strong>"
        },
        {
            "id": "5",
            "text": "fa<strong>a</strong>lmudabbir<u>aa</u>ti amr<u>aa</u><strong>n</strong>"
        },
        {
            "id": "6",
            "text": "yawma tarjufu <strong>al</strong>rr<u>aa</u>jifa<strong>tu</strong>"
        },
        {
            "id": "7",
            "text": "tatba'uh<u>aa</u> <strong>al</strong>rr<u>aa</u>difa<strong>tu</strong>"
        },
        {
            "id": "8",
            "text": "quluubun yawma-i<u>dz</u>in w<u>aa</u>jifa<strong>tun</strong>"
        },
        {
            "id": "9",
            "text": "ab<u>shaa</u>ruh<u>aa</u> kh<u>aa</u>syi'a<strong>tun</strong>"
        },
        {
            "id": "10",
            "text": "yaquuluuna a-inn<u>aa</u> lamarduuduuna fii <strong>a</strong>l<u>haa</u>fira<strong>ti</strong>"
        },
        {
            "id": "11",
            "text": "a-i<u>dzaa</u> kunn<u>aa</u> 'i<u>zhaa</u>man nakhira<strong>tan</strong>"
        },
        {
            "id": "12",
            "text": "q<u>aa</u>luu tilka i<u>dz</u>an karratun kh<u>aa</u>sira<strong>tun</strong>"
        },
        {
            "id": "13",
            "text": "fa-innam<u>aa</u> hiya zajratun w<u>aah</u>ida<strong>tun</strong>"
        },
        {
            "id": "14",
            "text": "fa-i<u>dzaa</u> hum bi<strong>al</strong>ss<u>aa</u>hira<strong>ti</strong>"
        },
        {
            "id": "15",
            "text": "hal at<u>aa</u>ka <u>h</u>adiitsu muus<u>aa</u>"
        },
        {
            "id": "16",
            "text": "i<u>dz</u> n<u>aa</u>d<u>aa</u>hu rabbuhu bi<strong>a</strong>lw<u>aa</u>di <strong>a</strong>lmuqaddasi <u>th</u>uw<u>aa</u><strong>n</strong>"
        },
        {
            "id": "17",
            "text": "i<u>dz</u>hab il<u>aa</u> fir'awna innahu <u>th</u>agh<u>aa</u>"
        },
        {
            "id": "18",
            "text": "faqul hal laka il<u>aa</u> an tazakk<u>aa</u>"
        },
        {
            "id": "19",
            "text": "wa-ahdiyaka il<u>aa</u> rabbika fatakhsy<u>aa</u>"
        },
        {
            "id": "20",
            "text": "fa-ar<u>aa</u>hu <strong>a</strong>l-<u>aa</u>yata <strong>a</strong>lkubr<u>aa</u>"
        },
        {
            "id": "21",
            "text": "faka<u>dzdz</u>aba wa'a<u>shaa</u>"
        },
        {
            "id": "22",
            "text": "tsumma adbara yas'<u>aa</u>"
        },
        {
            "id": "23",
            "text": "fa<u>h</u>asyara fan<u>aa</u>d<u>aa</u>"
        },
        {
            "id": "24",
            "text": "faq<u>aa</u>la an<u>aa</u> rabbukumu <strong>a</strong>l-a'l<u>aa</u>"
        },
        {
            "id": "25",
            "text": "fa-akha<u>dz</u>ahu <strong>al</strong>l<u>aa</u>hu nak<u>aa</u>la <strong>a</strong>l-<u>aa</u>khirati wa<strong>a</strong>l-uul<u>aa</u>"
        },
        {
            "id": "26",
            "text": "inna fii <u>dzaa</u>lika la'ibratan liman yakhsy<u>aa</u>"
        },
        {
            "id": "27",
            "text": "a-antum asyaddu khalqan ami <strong>al</strong>ssam<u>aa</u>u ban<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "28",
            "text": "rafa'a samkah<u>aa</u> fasaww<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "29",
            "text": "wa-agh<u>th</u>asya laylah<u>aa</u> wa-akhraja <u>dh</u>u<u>haa</u>h<u>aa</u>"
        },
        {
            "id": "30",
            "text": "wa<strong>a</strong>l-ar<u>dh</u>a ba'da <u>dzaa</u>lika da<u>haa</u>h<u>aa</u>"
        },
        {
            "id": "31",
            "text": "akhraja minh<u>aa</u> m<u>aa</u>-ah<u>aa</u> wamar'<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "32",
            "text": "wa<strong>a</strong>ljib<u>aa</u>la ars<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "33",
            "text": "mat<u>aa</u>'an lakum wali-an'<u>aa</u>mikum"
        },
        {
            "id": "34",
            "text": "fa-i<u>dzaa</u> j<u>aa</u>-ati <strong>al</strong><u>ththaa</u>mmatu <strong>a</strong>lkubr<u>aa</u>"
        },
        {
            "id": "35",
            "text": "yawma yata<u>dz</u>akkaru <strong>a</strong>l-ins<u>aa</u>nu m<u>aa</u> sa'<u>aa</u>"
        },
        {
            "id": "36",
            "text": "waburrizati <strong>a</strong>lja<u>h</u>iimu liman yar<u>aa</u>"
        },
        {
            "id": "37",
            "text": "fa-amm<u>aa</u> man <u>th</u>agh<u>aa</u>"
        },
        {
            "id": "38",
            "text": "wa<u>aa</u>tsara <strong>a</strong>l<u>h</u>ay<u>aa</u>ta <strong>al</strong>dduny<u>aa</u>"
        },
        {
            "id": "39",
            "text": "fa-inna <strong>a</strong>lja<u>h</u>iima hiya <strong>a</strong>lma/w<u>aa</u>"
        },
        {
            "id": "40",
            "text": "wa-am<u>aa</u> man kh<u>aa</u>fa maq<u>aa</u>ma rabbihi wanah<u>aa</u> <strong>al</strong>nnafsa 'ani <strong>a</strong>lhaw<u>aa</u>"
        },
        {
            "id": "41",
            "text": "fa-inna <strong>a</strong>ljannata hiya <strong>a</strong>lma/w<u>aa</u>"
        },
        {
            "id": "42",
            "text": "yas-aluunaka 'ani <strong>al</strong>ss<u>aa</u>'ati ayy<u>aa</u>na murs<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "43",
            "text": "fiima anta min <u>dz</u>ikr<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "44",
            "text": "il<u>aa</u> rabbika muntah<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "45",
            "text": "innam<u>aa</u> anta mun<u>dz</u>iru man yakhsy<u>aa</u>h<u>aa</u>"
        },
        {
            "id": "46",
            "text": "ka-annahum yawma yarawnah<u>aa</u> lam yalbatsuu ill<u>aa</u> 'asyiyyatan aw <u>dh</u>u<u>haa</u>h"
        }
    ]
}