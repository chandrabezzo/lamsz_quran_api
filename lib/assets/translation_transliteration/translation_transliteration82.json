{
    "id": "82",
    "name": "الإنفطار",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "i<u>dzaa</u> <strong>al</strong>ssam<u>aa</u>u infa<u>th</u>arath"
        },
        {
            "id": "2",
            "text": "wa-i<u>dzaa</u> <strong>a</strong>lkaw<u>aa</u>kibu intatsarath"
        },
        {
            "id": "3",
            "text": "wa-i<u>dzaa</u> <strong>a</strong>lbi<u>haa</u>ru fujjirath"
        },
        {
            "id": "4",
            "text": "wa-i<u>dzaa</u> <strong>a</strong>lqubuuru bu'tsirath"
        },
        {
            "id": "5",
            "text": "'alimat nafsun m<u>aa</u> qaddamat wa-akhkharath"
        },
        {
            "id": "6",
            "text": "y<u>aa</u> ayyuh<u>aa</u> <strong>a</strong>l-ins<u>aa</u>nu m<u>aa</u> gharraka birabbika <strong>a</strong>lkariim<strong>i</strong>"
        },
        {
            "id": "7",
            "text": "<strong>al</strong>la<u>dz</u>ii khalaqaka fasaww<u>aa</u>ka fa'adalak<strong>a</strong>"
        },
        {
            "id": "8",
            "text": "fii ayyi <u>sh</u>uuratin m<u>aa</u> sy<u>aa</u>-a rakkabak<strong>a</strong>"
        },
        {
            "id": "9",
            "text": "kall<u>aa</u> bal tuka<u>dzdz</u>ibuuna bi<strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "10",
            "text": "wa-inna 'alaykum la<u>haa</u>fi<u>zh</u>iin<strong>a</strong>"
        },
        {
            "id": "11",
            "text": "kir<u>aa</u>man k<u>aa</u>tibiin<strong>a</strong>"
        },
        {
            "id": "12",
            "text": "ya'lamuuna m<u>aa</u> taf'aluun<strong>a</strong>"
        },
        {
            "id": "13",
            "text": "inna <strong>a</strong>l-abr<u>aa</u>ra lafii na'iim<strong>in</strong>"
        },
        {
            "id": "14",
            "text": "wa-inna <strong>a</strong>lfujj<u>aa</u>ra lafii ja<u>h</u>iim<strong>in</strong>"
        },
        {
            "id": "15",
            "text": "ya<u>sh</u>lawnah<u>aa</u> yawma <strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "16",
            "text": "wam<u>aa</u> hum 'anh<u>aa</u> bigh<u>aa</u>-ibiin<strong>a</strong>"
        },
        {
            "id": "17",
            "text": "wam<u>aa</u> adr<u>aa</u>ka m<u>aa</u> yawmu <strong>al</strong>ddiini"
        },
        {
            "id": "18",
            "text": "tsumma m<u>aa</u> adr<u>aa</u>ka m<u>aa</u> yawmu <strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "19",
            "text": "yawma l<u>aa</u> tamliku nafsun linafsin syay-an wa<strong>a</strong>l-amru yawma-i<u>dz</u>in lill<u>aa</u>h<strong>i</strong>"
        }
    ]
}