{
    "id": "112",
    "name": "الإخلاص",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "Say: \"He is Allah, One.\""
        },
        {
            "id": "2",
            "text": "\"Allah As-Samad.\""
        },
        {
            "id": "3",
            "text": "\"He begets not, nor was He begotten.\""
        },
        {
            "id": "4",
            "text": "\"And there is none comparable to Him.\""
        }
    ]
}