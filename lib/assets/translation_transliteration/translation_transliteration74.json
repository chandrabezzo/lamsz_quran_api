{
    "id": "74",
    "name": "المدثر",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "y<u>aa</u> ayyuh<u>aa</u> <strong>a</strong>lmuddatstsir<strong>u</strong>"
        },
        {
            "id": "2",
            "text": "qum fa-an<u>dz</u>ir"
        },
        {
            "id": "3",
            "text": "warabbaka fakabbir"
        },
        {
            "id": "4",
            "text": "watsiy<u>aa</u>baka fa<u>th</u>ahhir"
        },
        {
            "id": "5",
            "text": "wa<strong>al</strong>rrujza fa<strong>u</strong>hjur"
        },
        {
            "id": "6",
            "text": "wal<u>aa</u> tamnun tastaktsir<strong>u</strong>"
        },
        {
            "id": "7",
            "text": "walirabbika fa<strong>i</strong><u>sh</u>bir"
        },
        {
            "id": "8",
            "text": "fa-i<u>dzaa</u> nuqira fii <strong>al</strong>nn<u>aa</u>quur<strong>i</strong>"
        },
        {
            "id": "9",
            "text": "fa<u>dzal</u>ika yawma-i<u>dz</u>in yawmun 'asiir<strong>un</strong>"
        },
        {
            "id": "10",
            "text": "'al<u>aa</u> <strong>a</strong>lk<u>aa</u>firiina ghayru yasiir<strong>in</strong>"
        },
        {
            "id": "11",
            "text": "<u>dz</u>arnii waman khalaqtu wa<u>h</u>iid<u>aa</u><strong>n</strong>"
        },
        {
            "id": "12",
            "text": "waja'altu lahu m<u>aa</u>lan mamduud<u>aa</u><strong>n</strong>"
        },
        {
            "id": "13",
            "text": "wabaniina syuhuud<u>aa</u><strong>n</strong>"
        },
        {
            "id": "14",
            "text": "wamahhadtu lahu tamhiid<u>aa</u><strong>n</strong>"
        },
        {
            "id": "15",
            "text": "tsumma ya<u>th</u>ma'u an aziid<strong>a</strong>"
        },
        {
            "id": "16",
            "text": "kall<u>aa</u> innahu k<u>aa</u>na li-<u>aa</u>y<u>aa</u>tin<u>aa</u> 'aniid<u>aa</u><strong>n</strong>"
        },
        {
            "id": "17",
            "text": "saurhiquhu <u>sh</u>a'uud<u>aa</u><strong>n</strong>"
        },
        {
            "id": "18",
            "text": "innahu fakkara waqaddar<strong>a</strong>"
        },
        {
            "id": "19",
            "text": "faqutila kayfa qaddar<strong>a</strong>"
        },
        {
            "id": "20",
            "text": "tsumma qutila kayfa qaddar<strong>a</strong>"
        },
        {
            "id": "21",
            "text": "tsumma na<u>zh</u>ar<strong>a</strong>"
        },
        {
            "id": "22",
            "text": "tsumma 'abasa wabasar<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "tsumma adbara wa<strong>i</strong>stakbar<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "faq<u>aa</u>la in h<u>aadzaa</u> ill<u>aa</u> si<u>h</u>run yu/tsar<strong>u</strong>"
        },
        {
            "id": "25",
            "text": "in h<u>aadzaa</u> ill<u>aa</u> qawlu <strong>a</strong>lbasyar<strong>i</strong>"
        },
        {
            "id": "26",
            "text": "sau<u>sh</u>liihi saqar<strong>a</strong>"
        },
        {
            "id": "27",
            "text": "wam<u>aa</u> adr<u>aa</u>ka m<u>aa</u> saqar<strong>u</strong>"
        },
        {
            "id": "28",
            "text": "l<u>aa</u> tubqii wal<u>aa</u> ta<u>dz</u>ar<strong>u</strong>"
        },
        {
            "id": "29",
            "text": "laww<u>aah</u>atun lilbasyar<strong>i</strong>"
        },
        {
            "id": "30",
            "text": "'alayh<u>aa</u> tis'ata 'asyar<strong>a</strong>"
        },
        {
            "id": "31",
            "text": "wam<u>aa</u> ja'aln<u>aa</u> a<u>sh</u>-<u>haa</u>ba <strong>al</strong>nn<u>aa</u>ri ill<u>aa</u> mal<u>aa</u>-ikatan wam<u>aa</u> ja'aln<u>aa</u> 'iddatahum ill<u>aa</u> fitnatan lilla<u>dz</u>iina kafaruu liyastayqina <strong>al</strong>la<u>dz</u>iina uutuu <strong>a</strong>lkit<u>aa</u>ba wayazd<u>aa</u>da <strong>al</strong>la<u>dz</u>iina <u>aa</u>manuu iim<u>aa</u>nan wal<u>aa</u> yart<u>aa</u>ba <strong>al</strong>la<u>dz</u>iina uutuu <strong>a</strong>lkit<u>aa</u>ba wa<strong>a</strong>lmu/minuuna waliyaquula <strong>al</strong>la<u>dz</u>iina fii quluubihim mara<u>dh</u>un wa<strong>a</strong>lk<u>aa</u>firuuna m<u>aatsaa</u> ar<u>aa</u>da <strong>al</strong>l<u>aa</u>hu bih<u>aadzaa</u> matsalan ka<u>dzaa</u>lika yu<u>dh</u>illu <strong>al</strong>l<u>aa</u>hu man yasy<u>aa</u>u wayahdii man yasy<u>aa</u>u wam<u>aa</u> ya'lamu junuuda rabbika ill<u>aa</u> huwa wam<u>aa</u> hiya ill<u>aa</u> <u>dz</u>ikr<u>aa</u> lilbasyar<strong>i</strong>"
        },
        {
            "id": "32",
            "text": "kall<u>aa</u> wa<strong>a</strong>lqamar<strong>i</strong>"
        },
        {
            "id": "33",
            "text": "wa<strong>a</strong>llayli i<u>dz</u> adbar<strong>a</strong>"
        },
        {
            "id": "34",
            "text": "wa<strong>al</strong><u>shsh</u>ub<u>h</u>i i<u>dzaa</u> asfar<strong>a</strong>"
        },
        {
            "id": "35",
            "text": "innah<u>aa</u> la-i<u>h</u>d<u>aa</u> <strong>a</strong>lkubar<strong>i</strong>"
        },
        {
            "id": "36",
            "text": "na<u>dz</u>iiran lilbasyar<strong>i</strong>"
        },
        {
            "id": "37",
            "text": "liman sy<u>aa</u>-a minkum an yataqaddama aw yata-akhkhar<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "kullu nafsin bim<u>aa</u> kasabat rahiina<strong>tun</strong>"
        },
        {
            "id": "39",
            "text": "ill<u>aa</u> a<u>sh</u>-<u>haa</u>ba <strong>a</strong>lyamiin<strong>i</strong>"
        },
        {
            "id": "40",
            "text": "fii jann<u>aa</u>tin yatas<u>aa</u>-aluun<strong>a</strong>"
        },
        {
            "id": "41",
            "text": "'ani <strong>a</strong>lmujrimiin<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "m<u>aa</u> salakakum fii saqar<strong>a</strong>"
        },
        {
            "id": "43",
            "text": "q<u>aa</u>luu lam naku mina <strong>a</strong>lmu<u>sh</u>alliin<strong>a</strong>"
        },
        {
            "id": "44",
            "text": "walam naku nu<u>th</u>'imu <strong>a</strong>lmiskiin<strong>a</strong>"
        },
        {
            "id": "45",
            "text": "wakunn<u>aa</u> nakhuu<u>dh</u>u ma'a <strong>a</strong>lkh<u>aa</u>-i<u>dh</u>iin<strong>a</strong>"
        },
        {
            "id": "46",
            "text": "wakunn<u>aa</u> nuka<u>dzdz</u>ibu biyawmi <strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "47",
            "text": "<u>h</u>att<u>aa</u> at<u>aa</u>n<u>aa</u> <strong>a</strong>lyaqiin<strong>u</strong>"
        },
        {
            "id": "48",
            "text": "fam<u>aa</u> tanfa'uhum syaf<u>aa</u>'atu <strong>al</strong>sysy<u>aa</u>fi'iin<strong>a</strong>"
        },
        {
            "id": "49",
            "text": "fam<u>aa</u> lahum 'ani <strong>al</strong>tta<u>dz</u>kirati mu'ri<u>dh</u>iin<strong>a</strong>"
        },
        {
            "id": "50",
            "text": "ka-annahum <u>h</u>umurun mustanfira<strong>tun</strong>"
        },
        {
            "id": "51",
            "text": "farrat min qaswara<strong>tin</strong>"
        },
        {
            "id": "52",
            "text": "bal yuriidu kullu imri-in minhum an yu/t<u>aa</u> <u>sh</u>u<u>h</u>ufan munasysyara<strong>tan</strong>"
        },
        {
            "id": "53",
            "text": "kall<u>aa</u> bal l<u>aa</u> yakh<u>aa</u>fuuna <strong>a</strong>l-<u>aa</u>khira<strong>ta</strong>"
        },
        {
            "id": "54",
            "text": "kall<u>aa</u> innahu ta<u>dz</u>kira<strong>tun</strong>"
        },
        {
            "id": "55",
            "text": "faman sy<u>aa</u>-a <u>dz</u>akarah<strong>u</strong>"
        },
        {
            "id": "56",
            "text": "wam<u>aa</u> ya<u>dz</u>kuruuna ill<u>aa</u> an yasy<u>aa</u>-a <strong>al</strong>l<u>aa</u>hu huwa ahlu <strong>al</strong>ttaqw<u>aa</u> wa-ahlu <strong>a</strong>lmaghfirat<strong>i</strong>"
        }
    ]
}