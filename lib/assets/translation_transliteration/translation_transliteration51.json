{
    "id": "51",
    "name": "الذاريات",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "wa<strong>al</strong><u>dzdzaa</u>riy<u>aa</u>ti <u>dz</u>arw<u>aa</u><strong>n</strong>"
        },
        {
            "id": "2",
            "text": "fa<strong>a</strong>l<u>haa</u>mil<u>aa</u>ti wiqr<u>aa</u><strong>n</strong>"
        },
        {
            "id": "3",
            "text": "fa<strong>a</strong>lj<u>aa</u>riy<u>aa</u>ti yusr<u>aa</u><strong>n</strong>"
        },
        {
            "id": "4",
            "text": "fa<strong>a</strong>lmuqassim<u>aa</u>ti amr<u>aa</u><strong>n</strong>"
        },
        {
            "id": "5",
            "text": "innam<u>aa</u> tuu'aduuna la<u>shaa</u>diq<strong>un</strong>"
        },
        {
            "id": "6",
            "text": "wa-inna <strong>al</strong>ddiina law<u>aa</u>qi'<strong>un</strong>"
        },
        {
            "id": "7",
            "text": "wa<strong>al</strong>ssam<u>aa</u>-i <u>dzaa</u>ti <strong>a</strong>l<u>h</u>ubuk<strong>i</strong>"
        },
        {
            "id": "8",
            "text": "innakum lafii qawlin mukhtalif<strong>in</strong>"
        },
        {
            "id": "9",
            "text": "yu/faku 'anhu man ufik<strong>a</strong>"
        },
        {
            "id": "10",
            "text": "qutila <strong>a</strong>lkharr<u>aas</u>uun<strong>a</strong>"
        },
        {
            "id": "11",
            "text": "<strong>al</strong>la<u>dz</u>iina hum fii ghamratin s<u>aa</u>huun<strong>a</strong>"
        },
        {
            "id": "12",
            "text": "yas-aluuna ayy<u>aa</u>na yawmu <strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "13",
            "text": "yawma hum 'al<u>aa</u> <strong>al</strong>nn<u>aa</u>ri yuftanuun<strong>a</strong>"
        },
        {
            "id": "14",
            "text": "<u>dz</u>uuquu fitnatakum h<u>aadzaa</u> <strong>al</strong>la<u>dz</u>ii kuntum bihi tasta'jiluun<strong>a</strong>"
        },
        {
            "id": "15",
            "text": "inna <strong>a</strong>lmuttaqiina fii jann<u>aa</u>tin wa'uyuun<strong>in</strong>"
        },
        {
            "id": "16",
            "text": "<u>aa</u>khi<u>dz</u>iina m<u>aa</u> <u>aa</u>t<u>aa</u>hum rabbuhum innahum k<u>aa</u>nuu qabla <u>dzaa</u>lika mu<u>h</u>siniin<strong>a</strong>"
        },
        {
            "id": "17",
            "text": "k<u>aa</u>nuu qaliilan mina <strong>al</strong>layli m<u>aa</u> yahja'uun<strong>a</strong>"
        },
        {
            "id": "18",
            "text": "wabi<strong>a</strong>l-as<u>haa</u>ri hum yastaghfiruun<strong>a</strong>"
        },
        {
            "id": "19",
            "text": "wafii amw<u>aa</u>lihim <u>h</u>aqqun li<strong>l</strong>ss<u>aa</u>-ili wa<strong>a</strong>lma<u>h</u>ruum<strong>i</strong>"
        },
        {
            "id": "20",
            "text": "wafii <strong>a</strong>l-ar<u>dh</u>i <u>aa</u>y<u>aa</u>tun lilmuuqiniin<strong>a</strong>"
        },
        {
            "id": "21",
            "text": "wafii anfusikum afal<u>aa</u> tub<u>sh</u>iruun<strong>a</strong>"
        },
        {
            "id": "22",
            "text": "wafii <strong>al</strong>ssam<u>aa</u>-i rizqukum wam<u>aa</u> tuu'aduun<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "fawarabbi <strong>al</strong>ssam<u>aa</u>-i wa<strong>a</strong>l-ar<u>dh</u>i innahu la<u>h</u>aqqun mitsla m<u>aa</u> annakum tan<u>th</u>iquun<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "hal at<u>aa</u>ka <u>h</u>adiitsu <u>dh</u>ayfi ibr<u>aa</u>hiima <strong>a</strong>lmukramiin<strong>a</strong>"
        },
        {
            "id": "25",
            "text": "i<u>dz</u> dakhaluu 'alayhi faq<u>aa</u>luu sal<u>aa</u>man q<u>aa</u>la sal<u>aa</u>mun qawmun munkaruun<strong>a</strong>"
        },
        {
            "id": "26",
            "text": "far<u>aa</u>gha il<u>aa</u> ahlihi faj<u>aa</u>-a bi'ijlin samiin<strong>in</strong>"
        },
        {
            "id": "27",
            "text": "faqarrabahu ilayhim q<u>aa</u>la <strong>a</strong>l<u>aa</u> ta/kuluun<strong>a</strong>"
        },
        {
            "id": "28",
            "text": "fa-awjasa minhum khiifatan q<u>aa</u>luu l<u>aa</u> takhaf wabasysyaruuhu bighul<u>aa</u>min 'aliim<strong>in</strong>"
        },
        {
            "id": "29",
            "text": "fa-aqbalati imra-atuhu fii <u>sh</u>arratin fa<u>sh</u>akkat wajhah<u>aa</u> waq<u>aa</u>lat 'ajuuzun 'aqiim<strong>un</strong>"
        },
        {
            "id": "30",
            "text": "q<u>aa</u>luu ka<u>dzaa</u>liki q<u>aa</u>la rabbuki innahu huwa <strong>a</strong>l<u>h</u>akiimu <strong>a</strong>l'aliim<strong>u</strong>"
        },
        {
            "id": "31",
            "text": "q<u>aa</u>la fam<u>aa</u> kha<u>th</u>bukum ayyuh<u>aa</u> <strong>a</strong>lmursaluun<strong>a</strong>"
        },
        {
            "id": "32",
            "text": "q<u>aa</u>luu inn<u>aa</u> ursiln<u>aa</u> il<u>aa</u> qawmin mujrimiin<strong>a</strong>"
        },
        {
            "id": "33",
            "text": "linursila 'alayhim <u>h</u>ij<u>aa</u>ratan min <u>th</u>iin<strong>in</strong>"
        },
        {
            "id": "34",
            "text": "musawwamatan 'inda rabbika lilmusrifiin<strong>a</strong>"
        },
        {
            "id": "35",
            "text": "fa-akhrajn<u>aa</u> man k<u>aa</u>na fiih<u>aa</u> mina <strong>a</strong>lmu/miniin<strong>a</strong>"
        },
        {
            "id": "36",
            "text": "fam<u>aa</u> wajadn<u>aa</u> fiih<u>aa</u> ghayra baytin mina <strong>a</strong>lmuslimiin<strong>a</strong>"
        },
        {
            "id": "37",
            "text": "watarakn<u>aa</u> fiih<u>aa</u> <u>aa</u>yatan lilla<u>dz</u>iina yakh<u>aa</u>fuuna <strong>a</strong>l'a<u>dzaa</u>ba <strong>a</strong>l-aliim<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "wafii muus<u>aa</u> i<u>dz</u> arsa<strong>l</strong>n<u>aa</u>hu il<u>aa</u> fir'awna bisul<u>th</u><u>aa</u>nin mubiin<strong>in</strong>"
        },
        {
            "id": "39",
            "text": "fatawall<u>aa</u> biruknihi waq<u>aa</u>la s<u>aah</u>irun aw majnuun<strong>un</strong>"
        },
        {
            "id": "40",
            "text": "fa-akha<u>dz</u>n<u>aa</u>hu wajunuudahu fanaba<u>dz</u>n<u>aa</u>hum fii <strong>a</strong>lyammi wahuwa muliim<strong>un</strong>"
        },
        {
            "id": "41",
            "text": "wafii '<u>aa</u>din i<u>dz</u> arsaln<u>aa</u> 'alayhimu <strong>al</strong>rrii<u>h</u>a <strong>a</strong>l'aqiim<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "m<u>aa</u> ta<u>dz</u>aru min syay-in atat 'alayhi ill<u>aa</u> ja'alat-hu ka<strong>al</strong>rramiim<strong>i</strong>"
        },
        {
            "id": "43",
            "text": "wafii tsamuuda i<u>dz</u> qiila lahum tamatta'uu <u>h</u>att<u>aa</u> <u>h</u>iin<strong>in</strong>"
        },
        {
            "id": "44",
            "text": "fa'ataw 'an amri rabbihim fa-akha<u>dz</u>at-humu <strong>al</strong><u>shshaa</u>'iqatu wahum yan<u>zh</u>uruun<strong>a</strong>"
        },
        {
            "id": "45",
            "text": "fam<u>aa</u> ista<u>th</u><u>aa</u>'uu min qiy<u>aa</u>min wam<u>aa</u> k<u>aa</u>nuu munta<u>sh</u>iriin<strong>a</strong>"
        },
        {
            "id": "46",
            "text": "waqawma nuu<u>h</u>in min qablu innahum k<u>aa</u>nuu qawman f<u>aa</u>siqiin<strong>a</strong>"
        },
        {
            "id": "47",
            "text": "wa<strong>al</strong>ssam<u>aa</u>-a banayn<u>aa</u>h<u>aa</u> bi-aydin wa-inn<u>aa</u> lamuusi'uun<strong>a</strong>"
        },
        {
            "id": "48",
            "text": "wa<strong>a</strong>l-ar<u>dh</u>a farasyn<u>aa</u>h<u>aa</u> fani'ma <strong>a</strong>lm<u>aa</u>hiduun<strong>a</strong>"
        },
        {
            "id": "49",
            "text": "wamin kulli syay-in khalaqn<u>aa</u> zawjayni la'allakum ta<u>dz</u>akkaruun<strong>a</strong>"
        },
        {
            "id": "50",
            "text": "fafirruu il<u>aa</u> <strong>al</strong>l<u>aa</u>hi innii lakum minhu na<u>dz</u>iirun mubiin<strong>un</strong>"
        },
        {
            "id": "51",
            "text": "wal<u>aa</u> taj'aluu ma'a <strong>al</strong>l<u>aa</u>hi il<u>aa</u>han <u>aa</u>khara innii lakum minhu na<u>dz</u>iirun mubiin<strong>un</strong>"
        },
        {
            "id": "52",
            "text": "ka<u>dzaa</u>lika m<u>aa</u> at<u>aa</u> <strong>al</strong>la<u>dz</u>iina min qablihim min rasuulin ill<u>aa</u> q<u>aa</u>luu s<u>aah</u>irun aw majnuun<strong>un</strong>"
        },
        {
            "id": "53",
            "text": "ataw<u>aas</u>aw bihi bal hum qawmun <u>th</u><u>aa</u>ghuun<strong>a</strong>"
        },
        {
            "id": "54",
            "text": "fatawalla 'anhum fam<u>aa</u> anta bimaluum<strong>in</strong>"
        },
        {
            "id": "55",
            "text": "wa<u>dz</u>akkir fa-inna <strong>al</strong><u>dzdz</u>ikr<u>aa</u> tanfa'u <strong>a</strong>lmu/miniin<strong>a</strong>"
        },
        {
            "id": "56",
            "text": "wam<u>aa</u> khalaqtu <strong>a</strong>ljinna wa<strong>a</strong>l-insa ill<u>aa</u> liya'buduun<strong>i</strong>"
        },
        {
            "id": "57",
            "text": "m<u>aa</u> uriidu minhum min rizqin wam<u>aa</u> uriidu an yu<u>th</u>'imuun<strong>i</strong>"
        },
        {
            "id": "58",
            "text": "inna <strong>al</strong>l<u>aa</u>ha huwa <strong>al</strong>rrazz<u>aa</u>qu <u>dz</u>uu <strong>a</strong>lquwwati <strong>a</strong>lmatiin<strong>u</strong>"
        },
        {
            "id": "59",
            "text": "fa-inna lilla<u>dz</u>iina <u>zh</u>alamuu <u>dz</u>anuuban mitsla <u>dz</u>anuubi a<u>sh</u>-<u>haa</u>bihim fal<u>aa</u> yasta'jiluun<strong>a</strong>"
        },
        {
            "id": "60",
            "text": "fawaylun lilla<u>dz</u>iina kafaruu min yawmihimu <strong>al</strong>la<u>dz</u>ii yuu'aduun<strong>a</strong>"
        }
    ]
}