{
    "id": "89",
    "name": "الفجر",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "wa<strong>a</strong>lfajr<strong>i</strong>"
        },
        {
            "id": "2",
            "text": "walay<u>aa</u>lin 'asyr<strong>in</strong>"
        },
        {
            "id": "3",
            "text": "wa<strong>al</strong>sysyaf'i wa<strong>a</strong>lwatr<strong>i</strong>"
        },
        {
            "id": "4",
            "text": "wa<strong>a</strong>llayli i<u>dzaa</u> yasr<strong>i</strong>"
        },
        {
            "id": "5",
            "text": "hal fii <u>dzaa</u>lika qasamun li<u>dz</u>ii <u>h</u>ijr<strong>in</strong>"
        },
        {
            "id": "6",
            "text": "alam tara kayfa fa'ala rabbuka bi'<u>aa</u>d<strong>in</strong>"
        },
        {
            "id": "7",
            "text": "irama <u>dzaa</u>ti <strong>a</strong>l'im<u>aa</u>d<strong>i</strong>"
        },
        {
            "id": "8",
            "text": "<strong>al</strong>latii lam yukhlaq mitsluh<u>aa</u> fii <strong>a</strong>lbil<u>aa</u>d<strong>i</strong>"
        },
        {
            "id": "9",
            "text": "watsamuuda <strong>al</strong>la<u>dz</u>iina j<u>aa</u>buu <strong>al</strong><u>shsh</u>akhra bi<strong>a</strong>lw<u>aa</u>d<strong>i</strong>"
        },
        {
            "id": "10",
            "text": "wafir'awna <u>dz</u>ii <strong>a</strong>l-awt<u>aa</u>d<strong>i</strong>"
        },
        {
            "id": "11",
            "text": "<strong>al</strong>la<u>dz</u>iina <u>th</u>aghaw fii <strong>a</strong>lbil<u>aa</u>d<strong>i</strong>"
        },
        {
            "id": "12",
            "text": "fa-aktsaruu fiih<u>aa</u> <strong>a</strong>lfas<u>aa</u>d<strong>a</strong>"
        },
        {
            "id": "13",
            "text": "fa<u>sh</u>abba 'alayhim rabbuka saw<u>th</u>a 'a<u>dzaa</u>b<strong>in</strong>"
        },
        {
            "id": "14",
            "text": "inna rabbaka labi<strong>a</strong>lmir<u>shaa</u>d<strong>i</strong>"
        },
        {
            "id": "15",
            "text": "fa-amm<u>aa</u> <strong>a</strong>l-ins<u>aa</u>nu i<u>dzaa</u> m<u>aa</u> ibtal<u>aa</u>hu rabbuhu fa-akramahu wana''amahu fayaquulu rabbii akraman<strong>i</strong>"
        },
        {
            "id": "16",
            "text": "wa-amm<u>aa</u> i<u>dzaa</u> m<u>aa</u> ibtal<u>aa</u>hu faqadara 'alayhi rizqahu fayaquulu rabbii ah<u>aa</u>nan<strong>i</strong>"
        },
        {
            "id": "17",
            "text": "kall<u>aa</u> bal l<u>aa</u> tukrimuuna <strong>a</strong>lyatiim<strong>a</strong>"
        },
        {
            "id": "18",
            "text": "wal<u>aa</u> ta<u>hadd</u>uuna 'al<u>aa</u> <u>th</u>a'<u>aa</u>mi <strong>a</strong>lmiskiin<strong>i</strong>"
        },
        {
            "id": "19",
            "text": "wata/kuluuna <strong>al</strong>ttur<u>aa</u>tsa aklan lamm<u>aa</u><strong>n</strong>"
        },
        {
            "id": "20",
            "text": "watu<u>h</u>ibbuuna <strong>a</strong>lm<u>aa</u>la <u>h</u>ubban jamm<u>aa</u><strong>n</strong>"
        },
        {
            "id": "21",
            "text": "kall<u>aa</u> i<u>dzaa</u> dukkati <strong>a</strong>l-ar<u>dh</u>u dakkan dakk<u>aa</u><strong>n</strong>"
        },
        {
            "id": "22",
            "text": "waj<u>aa</u>-a rabbuka wa<strong>a</strong>lmalaku <u>sh</u>affan <u>sh</u>aff<u>aa</u><strong>n</strong>"
        },
        {
            "id": "23",
            "text": "wajii-a yawma-i<u>dz</u>in bijahannama yawma-i<u>dz</u>in yata<u>dz</u>akkaru <strong>a</strong>l-ins<u>aa</u>nu wa-ann<u>aa</u> lahu <strong>al</strong><u>dzdz</u>ikr<u>aa</u>"
        },
        {
            "id": "24",
            "text": "yaquulu y<u>aa</u> laytanii qaddamtu li<u>h</u>ay<u>aa</u>tii"
        },
        {
            "id": "25",
            "text": "fayawma-i<u>dz</u>in l<u>aa</u> yu'a<u>dzdz</u>ibu 'a<u>dzaa</u>bahu a<u>h</u>ad<strong>un</strong>"
        },
        {
            "id": "26",
            "text": "wal<u>aa</u> yuutsiqu wats<u>aa</u>qahu a<u>h</u>ad<strong>un</strong>"
        },
        {
            "id": "27",
            "text": "y<u>aa</u> ayyatuh<u>aa</u> <strong>al</strong>nnafsu <strong>a</strong>lmu<u>th</u>ma-inna<strong>tu</strong>"
        },
        {
            "id": "28",
            "text": "irji'ii il<u>aa</u> rabbiki r<u>aad</u>iyatan mar<u>dh</u>iyya<strong>tan</strong>"
        },
        {
            "id": "29",
            "text": "fa<strong>u</strong>dkhulii fii 'ib<u>aa</u>dii"
        },
        {
            "id": "30",
            "text": "wa<strong>u</strong>dkhulii jannatii"
        }
    ]
}