{
    "id": "44",
    "name": "الدخان",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "<u>haa</u>-miim"
        },
        {
            "id": "2",
            "text": "wa<strong>a</strong>lkit<u>aa</u>bi <strong>a</strong>lmubiin<strong>i</strong>"
        },
        {
            "id": "3",
            "text": "inn<u>aa</u> anzaln<u>aa</u>hu fii laylatin mub<u>aa</u>rakatin inn<u>aa</u> kunn<u>aa</u> mun<u>dz</u>iriin<strong>a</strong>"
        },
        {
            "id": "4",
            "text": "fiih<u>aa</u> yufraqu kullu amrin <u>h</u>akiim<strong>in</strong>"
        },
        {
            "id": "5",
            "text": "amran min 'indin<u>aa</u> inn<u>aa</u> kunn<u>aa</u> mursiliin<strong>a</strong>"
        },
        {
            "id": "6",
            "text": "ra<u>h</u>matan min rabbika innahu huwa <strong>al</strong>ssamii'u <strong>a</strong>l'aliim<strong>u</strong>"
        },
        {
            "id": "7",
            "text": "rabbi <strong>al</strong>ssam<u>aa</u>w<u>aa</u>ti wa<strong>a</strong>l-ar<u>dh</u>i wam<u>aa</u> baynahum<u>aa</u> in kuntum muuqiniin<strong>a</strong>"
        },
        {
            "id": "8",
            "text": "l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa yu<u>h</u>yii wayumiitu rabbukum warabbu <u>aa</u>b<u>aa</u>-ikumu <strong>a</strong>l-awwaliin<strong>a</strong>"
        },
        {
            "id": "9",
            "text": "bal hum fii syakkin yal'abuun<strong>a</strong>"
        },
        {
            "id": "10",
            "text": "fa<strong>i</strong>rtaqib yawma ta/tii <strong>al</strong>ssam<u>aa</u>u bidukh<u>aa</u>nin mubiin<strong>in</strong>"
        },
        {
            "id": "11",
            "text": "yaghsy<u>aa</u> <strong>al</strong>nn<u>aa</u>sa h<u>aadzaa</u> 'a<u>dzaa</u>bun <strong>a</strong>liim<strong>un</strong>"
        },
        {
            "id": "12",
            "text": "rabban<u>aa</u> iksyif 'ann<u>aa</u> <strong>a</strong>l'a<u>dzaa</u>ba inn<u>aa</u> mu/minuun<strong>a</strong>"
        },
        {
            "id": "13",
            "text": "ann<u>aa</u> lahumu <strong>al</strong><u>dzdz</u>ikr<u>aa</u> waqad j<u>aa</u>-ahum rasuulun mubiin<strong>un</strong>"
        },
        {
            "id": "14",
            "text": "tsumma tawallaw 'anhu waq<u>aa</u>luu mu'allamun majnuun<strong>un</strong>"
        },
        {
            "id": "15",
            "text": "inn<u>aa</u> k<u>aa</u>syifuu <strong>a</strong>l'a<u>dzaa</u>bi qaliilan innakum '<u>aa</u>-iduun<strong>a</strong>"
        },
        {
            "id": "16",
            "text": "yawma nab<u>th</u>isyu <strong>a</strong>lba<u>th</u>syata <strong>a</strong>lkubr<u>aa</u> inn<u>aa</u> muntaqimuun<strong>a</strong>"
        },
        {
            "id": "17",
            "text": "walaqad fatann<u>aa</u> qablahum qawma fir'awna waj<u>aa</u>-ahum rasuulun kariim<strong>un</strong>"
        },
        {
            "id": "18",
            "text": "an adduu ilayya 'ib<u>aa</u>da <strong>al</strong>l<u>aa</u>hi innii lakum rasuulun amiin<strong>un</strong>"
        },
        {
            "id": "19",
            "text": "wa-an l<u>aa</u> ta'luu 'al<u>aa</u> <strong>al</strong>l<u>aa</u>hi innii <u>aa</u>tiikum bisul<u>th</u><u>aa</u>nin mubiin<strong>in</strong>"
        },
        {
            "id": "20",
            "text": "wa-innii 'u<u>dz</u>tu birabbii warabbikum an tarjumuun<strong>i</strong>"
        },
        {
            "id": "21",
            "text": "wa-in lam tu/minuu lii fa<strong>i</strong>'taziluun<strong>i</strong>"
        },
        {
            "id": "22",
            "text": "fada'<u>aa</u> rabbahu anna h<u>aa</u>ul<u>aa</u>-i qawmun mujrimuun<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "fa-asri bi'ib<u>aa</u>dii laylan innakum muttaba'uun<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "wa<strong>u</strong>truki <strong>a</strong>lba<u>h</u>ra rahwan innahum jundun mughraquun<strong>a</strong>"
        },
        {
            "id": "25",
            "text": "kam tarakuu min jann<u>aa</u>tin wa'uyuun<strong>in</strong>"
        },
        {
            "id": "26",
            "text": "wazuruu'in wamaq<u>aa</u>min kariim<strong>in</strong>"
        },
        {
            "id": "27",
            "text": "wana'matin k<u>aa</u>nuu fiih<u>aa</u> f<u>aa</u>kihiin<strong>a</strong>"
        },
        {
            "id": "28",
            "text": "ka<u>dzaa</u>lika wa-awratsn<u>aa</u>h<u>aa</u> qawman <u>aa</u>khariin<strong>a</strong>"
        },
        {
            "id": "29",
            "text": "fam<u>aa</u> bakat 'alayhimu <strong>al</strong>ssam<u>aa</u>u wa<strong>a</strong>l-ar<u>dh</u>u wam<u>aa</u> k<u>aa</u>nuu mun<u>zh</u>ariin<strong>a</strong>"
        },
        {
            "id": "30",
            "text": "walaqad najjayn<u>aa</u> banii isr<u>aa</u>-iila mina <strong>a</strong>l'a<u>dzaa</u>bi <strong>a</strong>lmuhiin<strong>i</strong>"
        },
        {
            "id": "31",
            "text": "min fir'awna innahu k<u>aa</u>na '<u>aa</u>liyan mina <strong>a</strong>lmusrifiin<strong>a</strong>"
        },
        {
            "id": "32",
            "text": "walaqadi ikhtarn<u>aa</u>hum 'al<u>aa</u> 'ilmin 'al<u>aa</u> <strong>a</strong>l'<u>aa</u>lamiin<strong>a</strong>"
        },
        {
            "id": "33",
            "text": "wa<u>aa</u>tayn<u>aa</u>hum mina <strong>a</strong>l-<u>aa</u>y<u>aa</u>ti m<u>aa</u> fiihi bal<u>aa</u>un mubiin<strong>un</strong>"
        },
        {
            "id": "34",
            "text": "inna h<u>aa</u>ul<u>aa</u>-i layaquuluun<strong>a</strong>"
        },
        {
            "id": "35",
            "text": "in hiya ill<u>aa</u> mawtatun<u>aa</u> <strong>a</strong>l-uul<u>aa</u> wam<u>aa</u> na<u>h</u>nu bimunsyariin<strong>a</strong>"
        },
        {
            "id": "36",
            "text": "fa/tuu bi-<u>aa</u>b<u>aa</u>-in<u>aa</u> in kuntum <u>shaa</u>diqiin<strong>a</strong>"
        },
        {
            "id": "37",
            "text": "ahum khayrun am qawmu tubba'in wa<strong>a</strong>lla<u>dz</u>iina min qablihim ahlakn<u>aa</u>hum innahum k<u>aa</u>nuu mujrimiin<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "wam<u>aa</u> khalaqn<u>aa</u> <strong>al</strong>ssam<u>aa</u>w<u>aa</u>ti wa<strong>a</strong>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> l<u>aa</u>'ibiin<strong>a</strong>"
        },
        {
            "id": "39",
            "text": "m<u>aa</u> khalaqn<u>aa</u>hum<u>aa</u> ill<u>aa</u> bi<strong>a</strong>l<u>h</u>aqqi wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya'lamuun<strong>a</strong>"
        },
        {
            "id": "40",
            "text": "inna yawma <strong>a</strong>lfa<u>sh</u>li miiq<u>aa</u>tuhum ajma'iin<strong>a</strong>"
        },
        {
            "id": "41",
            "text": "yawma l<u>aa</u> yughnii mawlan 'an mawlan syay-an wal<u>aa</u> hum yun<u>sh</u>aruun<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "ill<u>aa</u> man ra<u>h</u>ima <strong>al</strong>l<u>aa</u>hu innahu huwa <strong>a</strong>l'aziizu <strong>al</strong>rra<u>h</u>iim<strong>u</strong>"
        },
        {
            "id": "43",
            "text": "inna syajarata <strong>al</strong>zzaqquum<strong>i</strong>"
        },
        {
            "id": "44",
            "text": "<u>th</u>a'<u>aa</u>mu <strong>a</strong>l-atsiim<strong>i</strong>"
        },
        {
            "id": "45",
            "text": "ka<strong>a</strong>lmuhli yaghlii fii <strong>a</strong>lbu<u>th</u>uun<strong>i</strong>"
        },
        {
            "id": "46",
            "text": "kaghalyi <strong>a</strong>l<u>h</u>amiim<strong>i</strong>"
        },
        {
            "id": "47",
            "text": "khu<u>dz</u>uuhu fa<strong>i</strong>'tiluuhu il<u>aa</u> saw<u>aa</u>-i <strong>a</strong>lja<u>h</u>iim<strong>i</strong>"
        },
        {
            "id": "48",
            "text": "tsumma <u>sh</u>ubbuu fawqa ra/sihi min 'a<u>dzaa</u>bi <strong>a</strong>l<u>h</u>amiim<strong>i</strong>"
        },
        {
            "id": "49",
            "text": "<u>dz</u>uq innaka anta <strong>a</strong>l'aziizu <strong>a</strong>lkariim<strong>u</strong>"
        },
        {
            "id": "50",
            "text": "inna h<u>aadzaa</u> m<u>aa</u> kuntum bihi tamtaruun<strong>a</strong>"
        },
        {
            "id": "51",
            "text": "inna <strong>a</strong>lmuttaqiina fii maq<u>aa</u>min amiin<strong>in</strong>"
        },
        {
            "id": "52",
            "text": "fii jann<u>aa</u>tin wa'uyuun<strong>in</strong>"
        },
        {
            "id": "53",
            "text": "yalbasuuna min sundusin wa-istabraqin mutaq<u>aa</u>biliin<strong>a</strong>"
        },
        {
            "id": "54",
            "text": "ka<u>dzaa</u>lika wazawwajn<u>aa</u>hum bi<u>h</u>uurin 'iin<strong>in</strong>"
        },
        {
            "id": "55",
            "text": "yad'uuna fiih<u>aa</u> bikulli f<u>aa</u>kihatin <u>aa</u>miniin<strong>a</strong>"
        },
        {
            "id": "56",
            "text": "l<u>aa</u> ya<u>dz</u>uuquuna fiih<u>aa</u> <strong>a</strong>lmawta ill<u>aa</u> <strong>a</strong>lmawtata <strong>a</strong>l-uul<u>aa</u> wawaq<u>aa</u>hum 'a<u>dzaa</u>ba <strong>a</strong>lja<u>h</u>iim<strong>i</strong>"
        },
        {
            "id": "57",
            "text": "fa<u>dh</u>lan min rabbika <u>dzaa</u>lika huwa <strong>a</strong>lfawzu <strong>a</strong>l'a<u>zh</u>iim<strong>u</strong>"
        },
        {
            "id": "58",
            "text": "fa-innam<u>aa</u> yassarn<u>aa</u>hu bilis<u>aa</u>nika la'allahum yata<u>dz</u>akkaruun<strong>a</strong>"
        },
        {
            "id": "59",
            "text": "fa-innam<u>aa</u> yassarn<u>aa</u>hu bilis<u>aa</u>nika la'allahum yata<u>dz</u>akkaruun<strong>a</strong>"
        }
    ]
}