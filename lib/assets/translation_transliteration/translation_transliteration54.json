{
    "id": "54",
    "name": "القمر",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "iqtarabati <strong>al</strong>ss<u>aa</u>'atu wa<strong>i</strong>nsyaqqa <strong>a</strong>lqamar<strong>u</strong>"
        },
        {
            "id": "2",
            "text": "wa-in yaraw <u>aa</u>yatan yu'ri<u>dh</u>uu wayaquuluu si<u>h</u>run mustamirr<strong>un</strong>"
        },
        {
            "id": "3",
            "text": "waka<u>dzdz</u>abuu wa<strong>i</strong>ttaba'uu ahw<u>aa</u>-ahum wakullu amrin mustaqirr<strong>un</strong>"
        },
        {
            "id": "4",
            "text": "walaqad j<u>aa</u>-ahum mina <strong>a</strong>l-anb<u>aa</u>-i m<u>aa</u> fiihi muzdajar<strong>un</strong>"
        },
        {
            "id": "5",
            "text": "<u>h</u>ikmatun b<u>aa</u>lighatun fam<u>aa</u> tughnii <strong>al</strong>nnu<u>dz</u>ur<strong>u</strong>"
        },
        {
            "id": "6",
            "text": "fatawalla 'anhum yawma yad'u <strong>al</strong>dd<u>aa</u>'i il<u>aa</u> syay-in nukur<strong>in</strong>"
        },
        {
            "id": "7",
            "text": "khusysya'an ab<u>shaa</u>ruhum yakhrujuuna mina <strong>a</strong>l-ajd<u>aa</u>tsi ka-annahum jar<u>aa</u>dun muntasyir<strong>un</strong>"
        },
        {
            "id": "8",
            "text": "muh<u>th</u>i'iina il<u>aa</u> <strong>al</strong>dd<u>aa</u>'i yaquulu <strong>a</strong>lk<u>aa</u>firuuna h<u>aadzaa</u> yawmun 'asir<strong>un</strong>"
        },
        {
            "id": "9",
            "text": "ka<u>dzdz</u>abat qablahum qawmu nuu<u>h</u>in faka<u>dzdz</u>abuu 'abdan<u>aa</u> waq<u>aa</u>luu majnuunun wa<strong>i</strong>zdujir<strong>a</strong>"
        },
        {
            "id": "10",
            "text": "fada'<u>aa</u> rabbahu annii maghluubun fa<strong>i</strong>nta<u>sh</u>ir"
        },
        {
            "id": "11",
            "text": "fafata<u>h</u>n<u>aa</u> abw<u>aa</u>ba <strong>al</strong>ssam<u>aa</u>-i bim<u>aa</u>-in munhamir<strong>in</strong>"
        },
        {
            "id": "12",
            "text": "wafajjarn<u>aa</u> <strong>a</strong>l-ar<u>dh</u>a 'uyuunan fa<strong>i</strong>ltaq<u>aa</u> <strong>a</strong>lm<u>aa</u>u 'al<u>aa</u> amrin qad qudir<strong>a</strong>"
        },
        {
            "id": "13",
            "text": "wa<u>h</u>amaln<u>aa</u>hu 'al<u>aa</u> <u>dzaa</u>ti <strong>a</strong>lw<u>aah</u>in wadusur<strong>in</strong>"
        },
        {
            "id": "14",
            "text": "tajrii bi-a'yunin<u>aa</u> jaz<u>aa</u>-an liman k<u>aa</u>na kufir<strong>a</strong>"
        },
        {
            "id": "15",
            "text": "walaqad tarakn<u>aa</u>h<u>aa</u> <u>aa</u>yatan fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "16",
            "text": "fakayfa k<u>aa</u>na 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "17",
            "text": "walaqad yassarn<u>aa</u> <strong>a</strong>lqur-<u>aa</u>na li<strong>l</strong><u>dzdz</u>ikri fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "18",
            "text": "ka<u>dzdz</u>abat '<u>aa</u>dun fakayfa k<u>aa</u>na 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "19",
            "text": "inn<u>aa</u> arsaln<u>aa</u> 'alayhim rii<u>h</u>an <u>sh</u>ar<u>sh</u>aran fii yawmi na<u>h</u>sin mustamirr<strong>in</strong>"
        },
        {
            "id": "20",
            "text": "tanzi'u <strong>al</strong>nn<u>aa</u>sa ka-annahum a'j<u>aa</u>zu nakhlin munqa'ir<strong>in</strong>"
        },
        {
            "id": "21",
            "text": "fakayfa k<u>aa</u>na 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "22",
            "text": "walaqad yassarn<u>aa</u> <strong>a</strong>lqur-<u>aa</u>na li<strong>l</strong><u>dzdz</u>ikri fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "23",
            "text": "ka<u>dzdz</u>abat tsamuudu bi<strong>al</strong>nnu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "24",
            "text": "faq<u>aa</u>luu abasyaran minn<u>aa</u> w<u>aah</u>idan nattabi'uhu inn<u>aa</u> i<u>dz</u>an lafii <u>dh</u>al<u>aa</u>lin wasu'ur<strong>in</strong>"
        },
        {
            "id": "25",
            "text": "aulqiya <strong>al</strong><u>dzdz</u>ikru 'alayhi min baynin<u>aa</u> bal huwa ka<u>dzdzaa</u>bun asyir<strong>un</strong>"
        },
        {
            "id": "26",
            "text": "saya'lamuuna ghadan mani <strong>a</strong>lka<u>dzdzaa</u>bu <strong>a</strong>l-asyir<strong>u</strong>"
        },
        {
            "id": "27",
            "text": "inn<u>aa</u> mursiluu <strong>al</strong>nn<u>aa</u>qati fitnatan lahum fa<strong>i</strong>rtaqibhum wa<strong>i</strong><u>sth</u>abir"
        },
        {
            "id": "28",
            "text": "wanabbi/hum anna <strong>a</strong>lm<u>aa</u>-a qismatun baynahum kullu syirbin mu<u>h</u>ta<u>dh</u>ar<strong>un</strong>"
        },
        {
            "id": "29",
            "text": "fan<u>aa</u>daw <u>shaah</u>ibahum fata'<u>aataa</u> fa'aqar<strong>a</strong>"
        },
        {
            "id": "30",
            "text": "fakayfa k<u>aa</u>na 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "31",
            "text": "inn<u>aa</u> arsaln<u>aa</u> 'alayhim <u>sh</u>ay<u>h</u>atan w<u>aah</u>idatan fak<u>aa</u>nuu kahasyiimi <strong>a</strong>lmu<u>h</u>ta<u>zh</u>ir<strong>i</strong>"
        },
        {
            "id": "32",
            "text": "walaqad yassarn<u>aa</u> <strong>a</strong>lqur-<u>aa</u>na li<strong>l</strong><u>dzdz</u>ikri fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "33",
            "text": "ka<u>dzdz</u>abat qawmu luu<u>th</u>in bi<strong>al</strong>nnu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "34",
            "text": "inn<u>aa</u> arsaln<u>aa</u> 'alayhim <u>has</u>iban ill<u>aa</u> <u>aa</u>la luu<u>th</u>in najjayn<u>aa</u>hum bisa<u>h</u>ar<strong>in</strong>"
        },
        {
            "id": "35",
            "text": "ni'matan min 'indin<u>aa</u> ka<u>dzaa</u>lika najzii man syakar<strong>a</strong>"
        },
        {
            "id": "36",
            "text": "walaqad an<u>dz</u>arahum ba<u>th</u>syatan<u>aa</u> fatam<u>aa</u>raw bi<strong>al</strong>nnu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "37",
            "text": "walaqad r<u>aa</u>waduuhu 'an <u>dh</u>ayfihi fa<u>th</u>amasn<u>aa</u> a'yunahum fa<u>dz</u>uuquu 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "38",
            "text": "walaqad <u>sh</u>abba<u>h</u>ahum bukratan 'a<u>dzaa</u>bun mustaqirr<strong>un</strong>"
        },
        {
            "id": "39",
            "text": "fa<u>dz</u>uuquu 'a<u>dzaa</u>bii wanu<u>dz</u>ur<strong>i</strong>"
        },
        {
            "id": "40",
            "text": "walaqad yassarn<u>aa</u> <strong>a</strong>lqur-<u>aa</u>na li<strong>l</strong><u>dzdz</u>ikri fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "41",
            "text": "walaqad j<u>aa</u>-a <u>aa</u>la fir'awna <strong>al</strong>nnu<u>dz</u>ur<strong>u</strong>"
        },
        {
            "id": "42",
            "text": "ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> kullih<u>aa</u> fa-akha<u>dz</u>n<u>aa</u>hum akh<u>dz</u>a 'aziizin muqtadir<strong>in</strong>"
        },
        {
            "id": "43",
            "text": "akuff<u>aa</u>rukum khayrun min ul<u>aa</u>-ikum am lakum bar<u>aa</u>-atun fii <strong>al</strong>zzubur<strong>i</strong>"
        },
        {
            "id": "44",
            "text": "am yaquuluuna na<u>h</u>nu jamii'un munta<u>sh</u>ir<strong>un</strong>"
        },
        {
            "id": "45",
            "text": "sayuhzamu <strong>a</strong>ljam'u wayuwalluuna <strong>al</strong>ddubur<strong>a</strong>"
        },
        {
            "id": "46",
            "text": "bali <strong>al</strong>ss<u>aa</u>'atu maw'iduhum wa<strong>al</strong>ss<u>aa</u>'atu adh<u>aa</u> wa-amar<strong>ru</strong>"
        },
        {
            "id": "47",
            "text": "inna <strong>a</strong>lmujrimiina fii <u>dh</u>al<u>aa</u>lin wasu'ur<strong>in</strong>"
        },
        {
            "id": "48",
            "text": "yawma yus<u>h</u>abuuna fii <strong>al</strong>nn<u>aa</u>ri 'al<u>aa</u> wujuuhihim <u>dz</u>uuquu massa saqar<strong>a</strong>"
        },
        {
            "id": "49",
            "text": "inn<u>aa</u> kulla syay-in khalaqn<u>aa</u>hu biqadar<strong>in</strong>"
        },
        {
            "id": "50",
            "text": "wam<u>aa</u> amrun<u>aa</u> ill<u>aa</u> w<u>aah</u>idatun kalam<u>h</u>in bi<strong>a</strong>lba<u>sh</u>ar<strong>i</strong>"
        },
        {
            "id": "51",
            "text": "walaqad ahlakn<u>aa</u> asyy<u>aa</u>'akum fahal min muddakir<strong>in</strong>"
        },
        {
            "id": "52",
            "text": "wakullu syay-in fa'aluuhu fii <strong>al</strong>zzubur<strong>i</strong>"
        },
        {
            "id": "53",
            "text": "wakullu <u>sh</u>aghiirin wakabiirin musta<u>th</u>ar<strong>un</strong>"
        },
        {
            "id": "54",
            "text": "inna <strong>a</strong>lmuttaqiina fii jann<u>aa</u>tin wanahar<strong>in</strong>"
        },
        {
            "id": "55",
            "text": "fii maq'adi <u>sh</u>idqin 'inda maliikin muqtadir<strong>in</strong>"
        }
    ]
}