{
    "id": "68",
    "name": "القلم",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "nuun wa<strong>a</strong>lqalami wam<u>aa</u> yas<u>th</u>uruun<strong>a</strong>"
        },
        {
            "id": "2",
            "text": "m<u>aa</u> anta bini'mati rabbika bimajnuun<strong>in</strong>"
        },
        {
            "id": "3",
            "text": "wa-inna laka la-ajran ghayra mamnuun<strong>in</strong>"
        },
        {
            "id": "4",
            "text": "wa-innaka la'al<u>aa</u> khuluqin 'a<u>zh</u>iim<strong>in</strong>"
        },
        {
            "id": "5",
            "text": "fasatub<u>sh</u>iru wayub<u>sh</u>iruun<strong>a</strong>"
        },
        {
            "id": "6",
            "text": "bi-ayyikumu <strong>a</strong>lmaftuun<strong>u</strong>"
        },
        {
            "id": "7",
            "text": "inna rabbaka huwa a'lamu biman <u>dh</u>alla 'an sabiilihi wahuwa a'lamu bi<strong>a</strong>lmuhtadiin<strong>a</strong>"
        },
        {
            "id": "8",
            "text": "fal<u>aa</u> tu<u>th</u>i'i <strong>a</strong>lmuka<u>dzdz</u>ibiin<strong>a</strong>"
        },
        {
            "id": "9",
            "text": "wadduu law tudhinu fayudhinuun<strong>a</strong>"
        },
        {
            "id": "10",
            "text": "wal<u>aa</u> tu<u>th</u>i' kulla <u>h</u>all<u>aa</u>fin mahiin<strong>in</strong>"
        },
        {
            "id": "11",
            "text": "hamm<u>aa</u>zin masysy<u>aa</u>-in binamiim<strong>in</strong>"
        },
        {
            "id": "12",
            "text": "mann<u>aa</u>'in lilkhayri mu'tadin atsiim<strong>in</strong>"
        },
        {
            "id": "13",
            "text": "'utullin ba'da <u>dzaa</u>lika zaniim<strong>in</strong>"
        },
        {
            "id": "14",
            "text": "an k<u>aa</u>na <u>dzaa</u> m<u>aa</u>lin wabaniin<strong>a</strong>"
        },
        {
            "id": "15",
            "text": "i<u>dzaa</u> tutl<u>aa</u> 'alayhi <u>aa</u>y<u>aa</u>tun<u>aa</u> q<u>aa</u>la as<u>aath</u>iiru <strong>a</strong>l-awwaliin<strong>a</strong>"
        },
        {
            "id": "16",
            "text": "sanasimuhu 'al<u>aa</u> <strong>a</strong>lkhur<u>th</u>uum<strong>i</strong>"
        },
        {
            "id": "17",
            "text": "inn<u>aa</u> balawn<u>aa</u>hum kam<u>aa</u> balawn<u>aa</u> a<u>sh</u>-<u>haa</u>ba <strong>a</strong>ljannati i<u>dz</u> aqsamuu laya<u>sh</u>rimunnah<u>aa</u> mu<u>sh</u>bi<u>h</u>iin<strong>a</strong>"
        },
        {
            "id": "18",
            "text": "wal<u>aa</u> yastatsnuun<strong>a</strong>"
        },
        {
            "id": "19",
            "text": "fa<u>th</u><u>aa</u>fa 'alayh<u>aa</u> <u>th</u><u>aa</u>-ifun min rabbika wahum n<u>aa</u>-imuun<strong>a</strong>"
        },
        {
            "id": "20",
            "text": "fa-a<u>sh</u>ba<u>h</u>at ka<strong>al</strong><u>shsh</u>ariim<strong>i</strong>"
        },
        {
            "id": "21",
            "text": "fatan<u>aa</u>daw mu<u>sh</u>bi<u>h</u>iin<strong>a</strong>"
        },
        {
            "id": "22",
            "text": "ani ighduu 'al<u>aa</u> <u>h</u>artsikum in kuntum <u>shaa</u>rimiin<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "fa<strong>i</strong>n<u>th</u>alaquu wahum yatakh<u>aa</u>fatuun<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "an l<u>aa</u> yadkhulannah<u>aa</u> <strong>a</strong>lyawma 'alaykum miskiin<strong>un</strong>"
        },
        {
            "id": "25",
            "text": "waghadaw 'al<u>aa</u> <u>h</u>ardin q<u>aa</u>diriin<strong>a</strong>"
        },
        {
            "id": "26",
            "text": "falamm<u>aa</u> ra-awh<u>aa</u> q<u>aa</u>luu inn<u>aa</u> la<u>daa</u>lluun<strong>a</strong>"
        },
        {
            "id": "27",
            "text": "bal na<u>h</u>nu ma<u>h</u>ruumuun<strong>a</strong>"
        },
        {
            "id": "28",
            "text": "q<u>aa</u>la awsa<u>th</u>uhum alam aqul lakum lawl<u>aa</u> tusabbi<u>h</u>uun<strong>a</strong>"
        },
        {
            "id": "29",
            "text": "q<u>aa</u>luu sub<u>haa</u>na rabbin<u>aa</u> inn<u>aa</u> kunn<u>aa</u> <u>zhaa</u>limiin<strong>a</strong>"
        },
        {
            "id": "30",
            "text": "fa-aqbala ba'<u>dh</u>uhum 'al<u>aa</u> ba'<u>dh</u>in yatal<u>aa</u>wamuun<strong>a</strong>"
        },
        {
            "id": "31",
            "text": "q<u>aa</u>luu y<u>aa</u> waylan<u>aa</u> inn<u>aa</u> kunn<u>aa</u> <u>th</u><u>aa</u>ghiin<strong>a</strong>"
        },
        {
            "id": "32",
            "text": "'as<u>aa</u> rabbun<u>aa</u> an yubdilan<u>aa</u> khayran minh<u>aa</u> inn<u>aa</u> il<u>aa</u> rabbin<u>aa</u> r<u>aa</u>ghibuun<strong>a</strong>"
        },
        {
            "id": "33",
            "text": "ka<u>dzaa</u>lika <strong>a</strong>l'a<u>dzaa</u>bu wala'a<u>dzaa</u>bu <strong>a</strong>l-<u>aa</u>khirati akbaru law k<u>aa</u>nuu ya'lamuun<strong>a</strong>"
        },
        {
            "id": "34",
            "text": "inna lilmuttaqiina 'inda rabbihim jann<u>aa</u>ti <strong>al</strong>nna'iim<strong>i</strong>"
        },
        {
            "id": "35",
            "text": "afanaj'alu <strong>a</strong>lmuslimiina ka<strong>a</strong>lmujrimiin<strong>a</strong>"
        },
        {
            "id": "36",
            "text": "m<u>aa</u> lakum kayfa ta<u>h</u>kumuun<strong>a</strong>"
        },
        {
            "id": "37",
            "text": "am lakum kit<u>aa</u>bun fiihi tadrusuun<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "inna lakum fiihi lam<u>aa</u> takhayyaruun<strong>a</strong>"
        },
        {
            "id": "39",
            "text": "am lakum aym<u>aa</u>nun 'alayn<u>aa</u> b<u>aa</u>lighatun il<u>aa</u> yawmi <strong>a</strong>lqiy<u>aa</u>mati inna lakum lam<u>aa</u> ta<u>h</u>kumuun<strong>a</strong>"
        },
        {
            "id": "40",
            "text": "salhum ayyuhum bi<u>dzaa</u>lika za'iim<strong>un</strong>"
        },
        {
            "id": "41",
            "text": "am lahum syurak<u>aa</u>u falya/tuu bisyurak<u>aa</u>-ihim in k<u>aa</u>nuu <u>shaa</u>diqiin<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "yawma yuksyafu 'an s<u>aa</u>qin wayud'awna il<u>aa</u> <strong>al</strong>ssujuudi fal<u>aa</u> yasta<u>th</u>ii'uun<strong>a</strong>"
        },
        {
            "id": "43",
            "text": "kh<u>aa</u>syi'atan ab<u>shaa</u>ruhum tarhaquhum <u>dz</u>illatun waqad k<u>aa</u>nuu yud'awna il<u>aa</u> <strong>al</strong>ssujuudi wahum s<u>aa</u>limuun<strong>a</strong>"
        },
        {
            "id": "44",
            "text": "fa<u>dz</u>arnii waman yuka<u>dzdz</u>ibu bih<u>aadzaa</u> <strong>a</strong>l<u>h</u>adiitsi sanastadrijuhum min <u>h</u>aytsu l<u>aa</u> ya'lamuun<strong>a</strong>"
        },
        {
            "id": "45",
            "text": "waumlii lahum inna kaydii matiin<strong>un</strong>"
        },
        {
            "id": "46",
            "text": "am tas-aluhum ajran fahum min maghramin mutsqaluun<strong>a</strong>"
        },
        {
            "id": "47",
            "text": "am 'indahumu <strong>a</strong>lghaybu fahum yaktubuun<strong>a</strong>"
        },
        {
            "id": "48",
            "text": "fa<strong>i</strong><u>sh</u>bir li<u>h</u>ukmi rabbika wal<u>aa</u> takun ka<u>shaah</u>ibi <strong>a</strong>l<u>h</u>uuti i<u>dz</u> n<u>aa</u>d<u>aa</u> wahuwa mak<u>zh</u>uum<strong>un</strong>"
        },
        {
            "id": "49",
            "text": "lawl<u>aa</u> an tad<u>aa</u>rakahu ni'matun min rabbihi lanubi<u>dz</u>a bi<strong>a</strong>l'ar<u>aa</u>-i wahuwa ma<u>dz</u>muum<strong>un</strong>"
        },
        {
            "id": "50",
            "text": "fa<strong>i</strong>jtab<u>aa</u>hu rabbuhu faja'alahu mina <strong>al</strong><u>shshaa</u>li<u>h</u>iin<strong>a</strong>"
        },
        {
            "id": "51",
            "text": "wa-in yak<u>aa</u>du <strong>al</strong>la<u>dz</u>iina kafaruu layuzliquunaka bi-ab<u>shaa</u>rihim lamm<u>aa</u> sami'uu <strong>al</strong><u>dzdz</u>ikra wayaquuluuna innahu lamajnuun<strong>un</strong>"
        },
        {
            "id": "52",
            "text": "wam<u>aa</u> huwa ill<u>aa</u> <u>dz</u>ikrun lil'<u>aa</u>lamiin<strong>a</strong>"
        }
    ]
}