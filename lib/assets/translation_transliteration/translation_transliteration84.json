    "ayaTranslation": [
        {
            "id": "1",
            "text": "i<u>dzaa</u> <strong>al</strong>ssam<u>aa</u>u insyaqqath"
        },
        {
            "id": "2",
            "text": "wa-a<u>dz</u>inat lirabbih<u>aa</u> wa<u>h</u>uqqath"
        },
        {
            "id": "3",
            "text": "wa-i<u>dzaa</u> <strong>a</strong>l-ar<u>dh</u>u muddath"
        },
        {
            "id": "4",
            "text": "wa-alqat m<u>aa</u> fiih<u>aa</u> watakhallath"
        },
        {
            "id": "5",
            "text": "wa-a<u>dz</u>inat lirabbih<u>aa</u> wa<u>h</u>uqqath"
        },
        {
            "id": "6",
            "text": "y<u>aa</u> ayyuh<u>aa</u> <strong>a</strong>l-ins<u>aa</u>nu innaka k<u>aa</u>di<u>h</u>un il<u>aa</u> rabbika kad<u>h</u>an famul<u>aa</u>qiih<strong>i</strong>"
        },
        {
            "id": "7",
            "text": "fa-am<u>aa</u> man uutiya kit<u>aa</u>bahu biyamiinih<strong>i</strong>"
        },
        {
            "id": "8",
            "text": "fasawfa yu<u>haa</u>sabu <u>h</u>is<u>aa</u>ban yasiir<u>aa</u><strong>n</strong>"
        },
        {
            "id": "9",
            "text": "wayanqalibu il<u>aa</u> ahlihi masruur<u>aa</u><strong>n</strong>"
        },
        {
            "id": "10",
            "text": "wa-amm<u>aa</u> man uutiya kit<u>aa</u>bahu war<u>aa</u>-a <u>zh</u>ahrih<strong>i</strong>"
        },
        {
            "id": "11",
            "text": "fasawfa yad'uu tsubuur<u>aa</u><strong>n</strong>"
        },
        {
            "id": "12",
            "text": "waya<u>sh</u>l<u>aa</u> sa'iir<u>aa</u><strong>n</strong>"
        },
        {
            "id": "13",
            "text": "innahu k<u>aa</u>na fii ahlihi masruur<u>aa</u><strong>n</strong>"
        },
        {
            "id": "14",
            "text": "innahu <u>zh</u>anna an lan ya<u>h</u>uur<strong>a</strong>"
        },
        {
            "id": "15",
            "text": "bal<u>aa</u> inna rabbahu k<u>aa</u>na bihi ba<u>sh</u>iir<u>aa</u><strong>n</strong>"
        },
        {
            "id": "16",
            "text": "fal<u>aa</u> uqsimu bi<strong>al</strong>sysyafaq<strong>i</strong>"
        },
        {
            "id": "17",
            "text": "wa<strong>a</strong>llayli wam<u>aa</u> wasaq<strong>a</strong>"
        },
        {
            "id": "18",
            "text": "wa<strong>a</strong>lqamari i<u>dzaa</u> ittasaq<strong>a</strong>"
        },
        {
            "id": "19",
            "text": "latarkabunna <u>th</u>abaqan 'an <u>th</u>abaq<strong>in</strong>"
        },
        {
            "id": "20",
            "text": "fam<u>aa</u> lahum l<u>aa</u> yu/minuun<strong>a</strong>"
        },
        {
            "id": "21",
            "text": "wa-i<u>dzaa</u> quri-a 'alayhimu <strong>a</strong>lqur-<u>aa</u>nu l<u>aa</u> yasjuduun<strong>a</strong>"
        },
        {
            "id": "22",
            "text": "bali <strong>al</strong>la<u>dz</u>iina kafaruu yuka<u>dzdz</u>ibuun<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "wa<strong>al</strong>l<u>aa</u>hu a'lamu bim<u>aa</u> yuu'uun<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "fabasysyirhum bi'a<u>dzaa</u>bin <strong>a</strong>liim<strong>in</strong>"
        },
        {
            "id": "25",
            "text": "ill<u>aa</u> <strong>al</strong>la<u>dz</u>iina <u>aa</u>manuu wa'amiluu <strong>al</strong><u>shshaa</u>li<u>haa</u>ti lahum ajrun ghayru mamnuun<strong>in</strong>"
        }
    ]
