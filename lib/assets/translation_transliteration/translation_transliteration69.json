{
    "id": "69",
    "name": "الحاقة",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "al<u>haa</u>qqa<strong>tu</strong>"
        },
        {
            "id": "2",
            "text": "m<u>aa</u> <strong>a</strong>l<u>haa</u>qqa<strong>tu</strong>"
        },
        {
            "id": "3",
            "text": "wam<u>aa</u> adr<u>aa</u>ka m<u>aa</u> <strong>a</strong>l<u>haa</u>qqa<strong>tu</strong>"
        },
        {
            "id": "4",
            "text": "ka<u>dzdz</u>abat tsamuudu wa'<u>aa</u>dun bi<strong>a</strong>lq<u>aa</u>ri'a<strong>ti</strong>"
        },
        {
            "id": "5",
            "text": "fa-amm<u>aa</u> tsamuudu fauhlikuu bi<strong>al</strong><u>ththaa</u>ghiya<strong>ti</strong>"
        },
        {
            "id": "6",
            "text": "wa-amm<u>aa</u> '<u>aa</u>dun fauhlikuu birii<u>h</u>in <u>sh</u>ar<u>sh</u>arin '<u>aa</u>tiya<strong>tin</strong>"
        },
        {
            "id": "7",
            "text": "wa-amm<u>aa</u> '<u>aa</u>dun fauhlikuu birii<u>h</u>in <u>sh</u>ar<u>sh</u>arin '<u>aa</u>tiya<strong>tin</strong>"
        },
        {
            "id": "8",
            "text": "fahal tar<u>aa</u> lahum min b<u>aa</u>qiya<strong>tin</strong>"
        },
        {
            "id": "9",
            "text": "waj<u>aa</u>-a fir'awnu waman qablahu wa<strong>a</strong>lmu/tafik<u>aa</u>tu bi<strong>a</strong>lkh<u>aath</u>i-a<strong>ti</strong>"
        },
        {
            "id": "10",
            "text": "fa'a<u>sh</u>aw rasuula rabbihim fa-akha<u>dz</u>ahum akh<u>dz</u>atan r<u>aa</u>biya<strong>tan</strong>"
        },
        {
            "id": "11",
            "text": "inn<u>aa</u> lamm<u>aa</u> <u>th</u>agh<u>aa</u> <strong>a</strong>lm<u>aa</u>u <u>h</u>amaln<u>aa</u>kum fii <strong>a</strong>lj<u>aa</u>riya<strong>ti</strong>"
        },
        {
            "id": "12",
            "text": "linaj'alah<u>aa</u> lakum ta<u>dz</u>kiratan wata'iyah<u>aa</u> u<u>dz</u>unun w<u>aa</u>'iya<strong>tun</strong>"
        },
        {
            "id": "13",
            "text": "fa-i<u>dzaa</u> nufikha fii <strong>al</strong><u>shsh</u>uuri nafkhatun w<u>aah</u>ida<strong>tun</strong>"
        },
        {
            "id": "14",
            "text": "wa<u>h</u>umilati <strong>a</strong>l-ar<u>dh</u>u wa<strong>a</strong>ljib<u>aa</u>lu fadukkat<u>aa</u> dakkatan w<u>aah</u>ida<strong>tan</strong>"
        },
        {
            "id": "15",
            "text": "fayawma-i<u>dz</u>in waqa'ati <strong>a</strong>lw<u>aa</u>qi'a<strong>tu</strong>"
        },
        {
            "id": "16",
            "text": "wa<strong>i</strong>nsyaqqati <strong>al</strong>ssam<u>aa</u>u fahiya yawma-i<u>dz</u>in w<u>aa</u>hiya<strong>tun</strong>"
        },
        {
            "id": "17",
            "text": "wa<strong>a</strong>lmalaku 'al<u>aa</u> arj<u>aa</u>-ih<u>aa</u> waya<u>h</u>milu 'arsya rabbika fawqahum yawma-i<u>dz</u>in tsam<u>aa</u>niya<strong>tun</strong>"
        },
        {
            "id": "18",
            "text": "yawma-i<u>dz</u>in tu'ra<u>dh</u>uuna l<u>aa</u> takhf<u>aa</u> minkum kh<u>aa</u>fiya<strong>tun</strong>"
        },
        {
            "id": "19",
            "text": "fa-amm<u>aa</u> man uutiya kit<u>aa</u>bahu biyamiinihi fayaquulu h<u>aa</u>umu iqrauu kit<u>aa</u>biyah"
        },
        {
            "id": "20",
            "text": "innii <u>zh</u>anantu annii mul<u>aa</u>qin <u>h</u>is<u>aa</u>biyah"
        },
        {
            "id": "21",
            "text": "fahuwa fii 'iisyatin r<u>aad</u>iya<strong>tin</strong>"
        },
        {
            "id": "22",
            "text": "fii jannatin '<u>aa</u>liya<strong>tin</strong>"
        },
        {
            "id": "23",
            "text": "qu<u>th</u>uufuh<u>aa</u> d<u>aa</u>niya<strong>tun</strong>"
        },
        {
            "id": "24",
            "text": "kuluu wa<strong>i</strong>syrabuu hanii-an bim<u>aa</u> aslaftum fii <strong>a</strong>l-ayy<u>aa</u>mi <strong>a</strong>lkh<u>aa</u>liya<strong>ti</strong>"
        },
        {
            "id": "25",
            "text": "wa-amm<u>aa</u> man uutiya kit<u>aa</u>bahu bisyim<u>aa</u>lihi fayaquulu y<u>aa</u> laytanii lam uuta kit<u>aa</u>biyah"
        },
        {
            "id": "26",
            "text": "walam adri m<u>aa</u> <u>h</u>is<u>aa</u>biyah"
        },
        {
            "id": "27",
            "text": "y<u>aa</u> laytah<u>aa</u> k<u>aa</u>nati <strong>a</strong>lq<u>aad</u>iya<strong>tu</strong>"
        },
        {
            "id": "28",
            "text": "m<u>aa</u> aghn<u>aa</u> 'annii m<u>aa</u>liyah"
        },
        {
            "id": "29",
            "text": "halaka 'annii sul<u>th</u><u>aa</u>niyah"
        },
        {
            "id": "30",
            "text": "khu<u>dz</u>uuhu faghulluuh<strong>u</strong>"
        },
        {
            "id": "31",
            "text": "tsumma <strong>a</strong>lja<u>h</u>iima <u>sh</u>alluuh<strong>u</strong>"
        },
        {
            "id": "32",
            "text": "tsumma fii silsilatin <u>dz</u>ar'uh<u>aa</u> sab'uuna <u>dz</u>ir<u>aa</u>'an fa<strong>u</strong>slukuuh<strong>u</strong>"
        },
        {
            "id": "33",
            "text": "innahu k<u>aa</u>na l<u>aa</u> yu/minu bi<strong>al</strong>l<u>aa</u>hi <strong>a</strong>l'a<u>zh</u>iim<strong>i</strong>"
        },
        {
            "id": "34",
            "text": "wal<u>aa</u> ya<u>h</u>u<u>dhdh</u>u 'al<u>aa</u> <u>th</u>a'<u>aa</u>mi <strong>a</strong>lmiskiin<strong>i</strong>"
        },
        {
            "id": "35",
            "text": "wal<u>aa</u> ya<u>h</u>u<u>dhdh</u>u 'al<u>aa</u> <u>th</u>a'<u>aa</u>mi <strong>a</strong>lmiskiin<strong>i</strong>"
        },
        {
            "id": "36",
            "text": "wal<u>aa</u> <u>th</u>a'<u>aa</u>mun ill<u>aa</u> min ghisliin<strong>in</strong>"
        },
        {
            "id": "37",
            "text": "l<u>aa</u> ya/kuluhu ill<u>aa</u> <strong>a</strong>lkh<u>aath</u>i-uun<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "fal<u>aa</u> uqsimu bim<u>aa</u> tub<u>sh</u>iruun<strong>a</strong>"
        },
        {
            "id": "39",
            "text": "wam<u>aa</u> l<u>aa</u> tub<u>sh</u>iruun<strong>a</strong>"
        },
        {
            "id": "40",
            "text": "innahu laqawlu rasuulin kariim<strong>in</strong>"
        },
        {
            "id": "41",
            "text": "wam<u>aa</u> huwa biqawli sy<u>aa</u>'irin qaliilan m<u>aa</u> tu/minuun<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "wal<u>aa</u> biqawli k<u>aa</u>hinin qaliilan m<u>aa</u> ta<u>dz</u>akkaruun<strong>a</strong>"
        },
        {
            "id": "43",
            "text": "tanziilun min rabbi <strong>a</strong>l'<u>aa</u>lamiin<strong>a</strong>"
        },
        {
            "id": "44",
            "text": "walaw taqawwala 'alayn<u>aa</u> ba'<u>dh</u>a <strong>a</strong>l-aq<u>aa</u>wiil<strong>i</strong>"
        },
        {
            "id": "45",
            "text": "la-akha<u>dz</u>n<u>aa</u> minhu bi<strong>a</strong>lyamiin<strong>i</strong>"
        },
        {
            "id": "46",
            "text": "tsumma laqa<u>th</u>a'n<u>aa</u> minhu <strong>a</strong>lwatiin<strong>a</strong>"
        },
        {
            "id": "47",
            "text": "fam<u>aa</u> minkum min a<u>h</u>adin 'anhu <u>haa</u>jiziin<strong>a</strong>"
        },
        {
            "id": "48",
            "text": "wa-innahu lata<u>dz</u>kiratun lilmuttaqiin<strong>a</strong>"
        },
        {
            "id": "49",
            "text": "wa-inn<u>aa</u> lana'lamu anna minkum muka<u>dzdz</u>ibiin<strong>a</strong>"
        },
        {
            "id": "50",
            "text": "wa-innahu la<u>h</u>asratun 'al<u>aa</u> <strong>a</strong>lk<u>aa</u>firiin<strong>a</strong>"
        },
        {
            "id": "51",
            "text": "wa-innahu la<u>h</u>aqqu <strong>a</strong>lyaqiin<strong>i</strong>"
        },
        {
            "id": "52",
            "text": "fasabbi<u>h</u> bi<strong>i</strong>smi rabbika <strong>a</strong>l'a<u>zh</u>iim<strong>i</strong>"
        }
    ]
}