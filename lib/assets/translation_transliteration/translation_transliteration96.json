{
    "id": "96",
    "name": "العلق",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "iqra/ bi-ismi rabbika <strong>al</strong>la<u>dz</u>ii khalaq<strong>a</strong>"
        },
        {
            "id": "2",
            "text": "khalaqa <strong>a</strong>l-ins<u>aa</u>na min 'alaq<strong>in</strong>"
        },
        {
            "id": "3",
            "text": "iqra/ warabbuka <strong>a</strong>l-akram<strong>u</strong>"
        },
        {
            "id": "4",
            "text": "<strong>al</strong>la<u>dz</u>ii 'allama bi<strong>a</strong>lqalam<strong>i</strong>"
        },
        {
            "id": "5",
            "text": "'allama <strong>a</strong>l-ins<u>aa</u>na m<u>aa</u> lam ya'lam"
        },
        {
            "id": "6",
            "text": "kall<u>aa</u> inna <strong>a</strong>l-ins<u>aa</u>na laya<u>th</u>gh<u>aa</u>"
        },
        {
            "id": "7",
            "text": "an ra<u>aa</u>hu istaghn<u>aa</u>"
        },
        {
            "id": "8",
            "text": "inna il<u>aa</u> rabbika <strong>al</strong>rruj'<u>aa</u>"
        },
        {
            "id": "9",
            "text": "ara-ayta <strong>al</strong>la<u>dz</u>ii yanh<u>aa</u>"
        },
        {
            "id": "10",
            "text": "'abdan i<u>dzaa</u> <u>sh</u>all<u>aa</u>"
        },
        {
            "id": "11",
            "text": "ara-ayta in k<u>aa</u>na 'al<u>aa</u> <strong>a</strong>lhud<u>aa</u>"
        },
        {
            "id": "12",
            "text": "aw amara bi<strong>al</strong>ttaqw<u>aa</u>"
        },
        {
            "id": "13",
            "text": "ara-ayta in ka<u>dzdz</u>aba watawall<u>aa</u>"
        },
        {
            "id": "14",
            "text": "alam ya'lam bi-anna <strong>al</strong>l<u>aa</u>ha yar<u>aa</u>"
        },
        {
            "id": "15",
            "text": "kall<u>aa</u> la-in lam yantahi lanasfa'an bi<strong>al</strong>nn<u>aas</u>iya<strong>ti</strong>"
        },
        {
            "id": "16",
            "text": "n<u>aas</u>iyatin k<u>aadz</u>ibatin kh<u>aath</u>i-a<strong>tin</strong>"
        },
        {
            "id": "17",
            "text": "falyad'u n<u>aa</u>diyah<strong>u</strong>"
        },
        {
            "id": "18",
            "text": "sanad'u <strong>al</strong>zzab<u>aa</u>niya<strong>ta</strong>"
        },
        {
            "id": "19",
            "text": "kall<u>aa</u> l<u>aa</u> tu<u>th</u>i'hu wa<strong>u</strong>sjud wa<strong>i</strong>qtarib"
        }
    ]
}