    "name": "المعارج",
    "remark": "",
    "ayaTranslation": [
        {
            "id": "1",
            "text": "sa-ala s<u>aa</u>-ilun bi'a<u>dzaa</u>bin w<u>aa</u>qi'<strong>in</strong>"
        },
        {
            "id": "2",
            "text": "lilk<u>aa</u>firiina laysa lahu d<u>aa</u>fi'<strong>un</strong>"
        },
        {
            "id": "3",
            "text": "mina <strong>al</strong>l<u>aa</u>hi <u>dz</u>ii <strong>a</strong>lma'<u>aa</u>rij<strong>i</strong>"
        },
        {
            "id": "4",
            "text": "ta'ruju <strong>a</strong>lmal<u>aa</u>-ikatu wa<strong>al</strong>rruu<u>h</u>u ilayhi fii yawmin k<u>aa</u>na miqd<u>aa</u>ruhu khamsiina <strong>a</strong>lfa sana<strong>tin</strong>"
        },
        {
            "id": "5",
            "text": "fa<strong>i</strong><u>sh</u>bir <u>sh</u>abran jamiil<u>aa</u><strong>n</strong>"
        },
        {
            "id": "6",
            "text": "innahum yarawnahu ba'iid<u>aa</u><strong>n</strong>"
        },
        {
            "id": "7",
            "text": "wanar<u>aa</u>hu qariib<u>aa</u><strong>n</strong>"
        },
        {
            "id": "8",
            "text": "yawma takuunu <strong>al</strong>ssam<u>aa</u>u ka<strong>a</strong>lmuhl<strong>i</strong>"
        },
        {
            "id": "9",
            "text": "watakuunu <strong>a</strong>ljib<u>aa</u>lu ka<strong>a</strong>l'ihn<strong>i</strong>"
        },
        {
            "id": "10",
            "text": "wal<u>aa</u> yas-alu <u>h</u>amiimun <u>h</u>amiim<u>aa</u><strong>n</strong>"
        },
        {
            "id": "11",
            "text": "yuba<u>shsh</u>aruunahum yawaddu <strong>a</strong>lmujrimu law yaftadii min 'a<u>dzaa</u>bi yawmi-i<u>dz</u>in bibaniih<strong>i</strong>"
        },
        {
            "id": "12",
            "text": "wa<u>shaah</u>ibatihi wa-akhiih<strong>i</strong>"
        },
        {
            "id": "13",
            "text": "wafa<u>sh</u>iilatihi <strong>al</strong>latii tu/wiih<strong>i</strong>"
        },
        {
            "id": "14",
            "text": "waman fii <strong>a</strong>l-ar<u>dh</u>i jamii'an tsumma yunjiih<strong>i</strong>"
        },
        {
            "id": "15",
            "text": "kall<u>aa</u> innah<u>aa</u> la<u>zhaa</u>"
        },
        {
            "id": "16",
            "text": "nazz<u>aa</u>'atan li<strong>l</strong>sysyaw<u>aa</u>"
        },
        {
            "id": "17",
            "text": "tad'uu man adbara watawall<u>aa</u>"
        },
        {
            "id": "18",
            "text": "wajama'a fa-aw'<u>aa</u>"
        },
        {
            "id": "19",
            "text": "inna <strong>a</strong>l-ins<u>aa</u>na khuliqa haluu'<u>aa</u><strong>n</strong>"
        },
        {
            "id": "20",
            "text": "i<u>dzaa</u> massahu <strong>al</strong>sysyarru jazuu'<u>aa</u><strong>n</strong>"
        },
        {
            "id": "21",
            "text": "wa-i<u>dzaa</u> massahu <strong>a</strong>lkhayru manuu'<u>aa</u><strong>n</strong>"
        },
        {
            "id": "22",
            "text": "ill<u>aa</u> <strong>a</strong>lmu<u>sh</u>alliin<strong>a</strong>"
        },
        {
            "id": "23",
            "text": "ill<u>aa</u> <strong>a</strong>lmu<u>sh</u>alliin<strong>a</strong>"
        },
        {
            "id": "24",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina fii amw<u>aa</u>lihim <u>h</u>aqqun ma'luum<strong>un</strong>"
        },
        {
            "id": "25",
            "text": "li<strong>l</strong>ss<u>aa</u>-ili wa<strong>a</strong>lma<u>h</u>ruum<strong>i</strong>"
        },
        {
            "id": "26",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina yu<u>sh</u>addiquuna biyawmi <strong>al</strong>ddiin<strong>i</strong>"
        },
        {
            "id": "27",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina hum min 'a<u>dzaa</u>bi rabbihim musyfiquun<strong>a</strong>"
        },
        {
            "id": "28",
            "text": "inna 'a<u>dzaa</u>ba rabbihim ghayru ma/muun<strong>in</strong>"
        },
        {
            "id": "29",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina hum lifuruujihim <u>haa</u>fi<u>zh</u>uun<strong>a</strong>"
        },
        {
            "id": "30",
            "text": "ill<u>aa</u> 'al<u>aa</u> azw<u>aa</u>jihim aw m<u>aa</u> malakat aym<u>aa</u>nuhum fa-innahum ghayru maluumiin<strong>a</strong>"
        },
        {
            "id": "31",
            "text": "famani ibtagh<u>aa</u> war<u>aa</u>-a <u>dzaa</u>lika faul<u>aa</u>-ika humu <strong>a</strong>l'<u>aa</u>duun<strong>a</strong>"
        },
        {
            "id": "32",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina hum li-am<u>aa</u>n<u>aa</u>tihim wa'ahdihim r<u>aa</u>'uun<strong>a</strong>"
        },
        {
            "id": "33",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina hum bisyah<u>aa</u>d<u>aa</u>tihim q<u>aa</u>-imuun<strong>a</strong>"
        },
        {
            "id": "34",
            "text": "wa<strong>a</strong>lla<u>dz</u>iina hum 'al<u>aa</u> <u>sh</u>al<u>aa</u>tihim yu<u>haa</u>fi<u>zh</u>uun<strong>a</strong>"
        },
        {
            "id": "35",
            "text": "ul<u>aa</u>-ika fii jann<u>aa</u>tin mukramuun<strong>a</strong>"
        },
        {
            "id": "36",
            "text": "fam<u>aa</u>li <strong>al</strong>la<u>dz</u>iina kafaruu qibalaka muh<u>th</u>i'iin<strong>a</strong>"
        },
        {
            "id": "37",
            "text": "'ani <strong>a</strong>lyamiini wa'ani <strong>al</strong>sysyim<u>aa</u>li 'iziin<strong>a</strong>"
        },
        {
            "id": "38",
            "text": "aya<u>th</u>ma'u kullu imri-in minhum an yudkhala jannata na'iim<strong>in</strong>"
        },
        {
            "id": "39",
            "text": "kall<u>aa</u> inn<u>aa</u> khalaqn<u>aa</u>hum mimm<u>aa</u> ya'lamuun<strong>a</strong>"
        },
        {
            "id": "40",
            "text": "fal<u>aa</u> uqsimu birabbi <strong>a</strong>lmasy<u>aa</u>riqi wa<strong>a</strong>lmagh<u>aa</u>ribi inn<u>aa</u> laq<u>aa</u>diruun<strong>a</strong>"
        },
        {
            "id": "41",
            "text": "'al<u>aa</u> an nubaddila khayran minhum wam<u>aa</u> na<u>h</u>nu bimasbuuqiin<strong>a</strong>"
        },
        {
            "id": "42",
            "text": "fa<u>dz</u>arhum yakhuu<u>dh</u>uu wayal'abuu <u>h</u>att<u>aa</u> yul<u>aa</u>quu yawmahumu <strong>al</strong>la<u>dz</u>ii yuu'aduun<strong>a</strong>"
        },
        {
            "id": "43",
            "text": "yawma yakhrujuuna mina <strong>a</strong>l-ajd<u>aa</u>tsi sir<u>aa</u>'an ka-annahum il<u>aa</u> nu<u>sh</u>ubin yuufi<u>dh</u>uun<strong>a</strong>"
        },
        {
            "id": "44",
            "text": "kh<u>aa</u>syi'atan ab<u>shaa</u>ruhum tarhaquhum <u>dz</u>illatun <u>dzaa</u>lika <strong>a</strong>lyawmu <strong>al</strong>la<u>dz</u>ii k<u>aa</u>nuu yuu'aduun<strong>a</strong>"
        }
    ]
